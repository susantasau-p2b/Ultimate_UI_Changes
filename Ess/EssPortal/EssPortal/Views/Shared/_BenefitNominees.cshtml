@{
    Layout = null;
}
@model P2b.Global.BenefitNominees

<style>
    .qual_popup-content-textbox {
        min-width: 200px;
        margin: 0px 0px 1px 3px;
    }

    .qual_popup-content-label {
        display: block;
        float: left;
        width: 165px;
    }

    .content_margine {
        margin: 2.5px;
        padding: 0.5px;
    }

    .qual_row3 {
        width: 473px;
    }

    .qual_icon_row3 {
        left: 118px;
        top: -150px;
        position: relative;
    }
</style>

<div class="lookup_Increment">
    <div title="LookUp Data">
        <div class="lookupdiv"></div>
    </div>
    <div id="pageNavPosition">

    </div>
</div>
<div id="create_dialog" title="Create PF and Gratuity Nomination Detail">
    @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frmBenefitNominees", name = "frmBenefitNominees" }))
    {
        <input id="auth_action" type="hidden" name="auth_action" />
        <input id="Add" type="hidden" name="Add" />
        <input id="autho_allow" type="hidden" name="autho_allow" />
        <input id="isauth" type="hidden" name="isauth" />
        <input id="id" type="hidden" name="auth_id" />
    <div class="qual_row3 content_margine">
        <div id="Nomineeslist_old"></div>
        @Html.LabelFor(m => m.NomineeName, "Nominees Name :", new { @class = "qual_popup-content-label" })
        <div class="popup-content-table">
            <table class="lookuptableselected" id="NomineeNamelist">
                <tr><th>Id</th><th>LookUp Selected Description</th></tr>
            </table>
        </div>
        <div class="qual_icon_row3 icon-row">
            <button data-p2btooltip="Lookup Nomineename" class="  popup-content-icon-lookup" id="lookup-NomineeName-BenefitNominees"><span class="ui-icon ui-icon-folder-open popup-content-iconmark"></span></button>
            <button data-p2btooltip="Create Nomineename" class="popup-content-icon-create" id="create-NomineeName-BenefitNominees"><span class="ui-icon ui-icon-newwin popup-content-iconmark"></span></button>
            <button data-p2btooltip="Edit Nomineename" class="popup-content-icon-edit" id="edit-NomineeName-BenefitNominees"><span class="ui-icon ui-icon-pencil popup-content-iconmark"></span></button>
            <button data-p2btooltip="Remove Nomineename" class="popup-content-icon-remove" id="remove-NomineeName-BenefitNominees"><span class="ui-icon ui-icon-trash popup-content-iconmark"></span></button>
        </div>
    </div>

        <div class="row3 content_margine">@Html.LabelFor(m => m.DateofBirth, "Date Birth :", new { @class = "qual_popup-content-label must" })@Html.TextBoxFor(m => m.DateofBirth, new { @class = "qual_popup-content-textbox must", @maxlength = "10", id = "DateofBirth" })</div>
        <div class="row3 content_margine">@Html.LabelFor(m => m.Relation, "Relation :", new { @class = "qual_popup-content-label must" })<select class="must" id="Categorylist" name="Categorylist" style="width:161px;"><option></option></select></div>
        <div class="qual_row3 content_margine">
            <div id="Addresslist_old"></div>
            @Html.LabelFor(m => m.Address, "Address :", new { @class = "qual_popup-content-label" })
            <div class="popup-content-table">
                <table class="lookuptableselected" id="Addresslist">
                    <tr><th>Id</th><th>LookUp Selected Description</th></tr>
                </table>
            </div>
            <div class="qual_icon_row3 icon-row">
                <button data-p2btooltip="Lookup Address" class="  popup-content-icon-lookup" id="lookup-Address-BenefitNominees"><span class="ui-icon ui-icon-folder-open popup-content-iconmark"></span></button>
                <button data-p2btooltip="Create Address" class="popup-content-icon-create" id="create-Address-BenefitNominees"><span class="ui-icon ui-icon-newwin popup-content-iconmark"></span></button>
                <button data-p2btooltip="Edit Address" class="popup-content-icon-edit" id="edit-Address-BenefitNominees"><span class="ui-icon ui-icon-pencil popup-content-iconmark"></span></button>
                <button data-p2btooltip="Remove Address" class="popup-content-icon-remove" id="remove-Address-BenefitNominees"><span class="ui-icon ui-icon-trash popup-content-iconmark"></span></button>
            </div>
        </div>
        <div class="qual_row3 content_margine">
            <div id="ContactDetailslist_old"></div>@Html.LabelFor(m => m.ContactDetails, "Contact Details :", new { @class = "qual_popup-content-label" })
            <div class="popup-content-table">
                <table class="lookuptableselected" id="ContactDetailslist">
                    <tr><th>Id</th><th>LookUp Selected Description</th></tr>
                </table>
            </div>
            <div class="qual_icon_row3 icon-row">
                <button data-p2btooltip="Lookup Contact Details" class="  popup-content-icon-lookup" id="lookup-ContactDetails-BenefitNominees"><span class="ui-icon ui-icon-folder-open popup-content-iconmark"></span></button>
                <button data-p2btooltip="Create Contact Details" class="  popup-content-icon-create" id="create-ContactDetails-BenefitNominees"><span class="ui-icon ui-icon-newwin popup-content-iconmark"></span></button>
                <button data-p2btooltip="Edit Contact Details" class="  popup-content-icon-edit" id="edit-ContactDetails-BenefitNominees"><span class="ui-icon ui-icon-pencil popup-content-iconmark"></span></button>
                <button data-p2btooltip="Remove Contact Details" class="  popup-content-icon-remove" id="remove-ContactDetails-BenefitNominees"><span class="ui-icon ui-icon-trash popup-content-iconmark"></span></button>
            </div>
        </div>
        <div class="qual_row3 content_margine">
            <div id="Benefit_old"></div>@Html.LabelFor(m => m.BenefitList, "Benefit List :", new { @class = "qual_popup-content-label" })
            <div class="popup-content-table">
                <table class="lookuptableselected" id="BenefitTypelist">
                    <tr><th>Id</th><th>LookUp Selected Description</th></tr>
                </table>
            </div>
            <div class="qual_icon_row3 icon-row">
                <button data-p2btooltip="Lookup BenefitList" class="popup-content-icon-lookup" id="lookup-BenefitTypelist-BenefitNominees"><span class="ui-icon ui-icon-folder-open popup-content-iconmark"></span></button>
                <button data-p2btooltip="Create BenefitList" class="popup-content-icon-create" id="create-BenefitTypelist-BenefitNominees"><span class="ui-icon ui-icon-newwin popup-content-iconmark"></span></button>
                <button data-p2btooltip="Edit BenefitList" class="popup-content-icon-edit" id="edit-BenefitTypelist-BenefitNominees"><span class="ui-icon ui-icon-pencil popup-content-iconmark"></span></button>
                <button data-p2btooltip="Remove BenefitList " class="popup-content-icon-remove" id="remove-BenefitTypelist-BenefitNominees"><span class="ui-icon ui-icon-trash popup-content-iconmark"></span></button>
            </div>
        </div>
    }
</div>

<script>
    $(document).ready(function (e) {
        $('button').button();
        $('#Categorylist').selectmenu().selectmenu("menuWidget");
        $('#Type_Nomines').selectmenu().selectmenu("menuWidget");

        function AssignData() {
            dt = $('#jqgrid').P2BGrid.P2BEdDelV('#jqgrid', 'Id');

        }
        $('#Categorylist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 107, ""); // To bind dropdown on page load

        $('#DateofBirth').P2BDatePicker();

        function DateConvert(JsonDate) {
            var date = new Date(parseInt(JsonDate.substr(6)));
            var displayDate = $.datepicker.formatDate("dd/mm/yy", date);
            return displayDate;
        };

        /* Address button click*/
        $('#lookup-Address-BenefitNominees').on('click', function (event) {
            $('.lookup_Increment').P2BLookUpModal('@Url.Action("GetAddressLKDetails", "BenefitNominees")', '', 'lookupdiv', 'Address Look Up Data', '#Addresslist', 'Addresslist', 5, 'Addresslist', '#frmBenefitNominees', 'N', '#edit-Address-BenefitNominees,#remove-Address-BenefitNominees', 'lookup_table', 'pageNavPosition');
            event.preventDefault();
        });

        // PArtial page create
        $('<div></div>').P2BPartialCreateModalDialog();
        $('#create-Address-BenefitNominees').on("click", function (event) {
