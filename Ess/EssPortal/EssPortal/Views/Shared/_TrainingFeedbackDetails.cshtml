@{
    Layout = null;
}
<style>
    .cont_popup-content-textbox {
        margin-bottom: 1px;
        min-width: 150px;
    }

    .cont_popup-content-label {
        display: block;
        margin: 1px;
        float: left;
        width: 160px;
    }

    .cont_row {
        width: 350px;
    }
</style>
<script>
    $(document).ready(function () {
        // $('#MobileNo').ForceOnlyNumeric();
        $('#OverallFaultyRating').ForceOnlyNumeric();
        $('#FacultyFeedback').ForceOnlyNumeric();
        $('#OverallTrainingRating').ForceOnlyNumeric();
        $('#OverallTrainingFeedback').ForceOnlyNumeric();
        
    });

</script>

@model Training.TrainingSchedule
<div>
    @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frmContactNo", name = "frmContactNo" }))
    {
        <div>@Html.LabelFor(m => m.OverallFaultyRating, "Overall FaultyRating :", new { @class = "cont_popup-content-label" })@Html.TextBoxFor(m => m.OverallFaultyRating, new { @class = "cont_popup-content-textbox"})</div>
        <div>@Html.LabelFor(m => m.FacultyFeedback, "Faculty Feedback :", new { @class = "cont_popup-content-label" })@Html.TextBoxFor(m => m.FacultyFeedback, new { @class = "cont_popup-content-textbox"})</div>
        
        <div>@Html.LabelFor(m => m.OverallTrainingRating, "Overall TrainingRating :", new { @class = "cont_popup-content-label" })@Html.TextBoxFor(m => m.OverallTrainingRating, new { @class = "cont_popup-content-textbox" })</div>
        <div>@Html.LabelFor(m => m.OverallTrainingFeedback, "Overall TrainingFeedback :", new { @class = "cont_popup-content-label" })@Html.TextBoxFor(m => m.OverallTrainingFeedback, new { @class = "cont_popup-content-textbox"})</div>
    }
</div>