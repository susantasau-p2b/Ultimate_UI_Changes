<script type="text/javascript">
    $(function () {
        $('.r-tooltip')._tooltip();
        $.get("@Url.Action("GetUserinfo", "Login")", function (data) {
            $('#company-name,#employe-name').empty();
            if (data.success == true) {
                if (data.data != null) {
                    var k = data.data;
                    if (k["GeoStruct_Company_Name"] != null) {
                        $('#company-name').html(k["GeoStruct_Company_Name"]);
                        $('#header-company-title').html(k["GeoStruct_Company_Name"]);
                    } else {
                        $('#company-name').html("<p></p>");
                    }
                }
            } else {
                console.log(data);
            }
        });

        function ajaxloaderv2(cameform) {
            $('<a><i class="fa fa-circle-o-notch fa-spin fa-5x fa-fw ajax_loder" style="color: rgba(27, 187, 173, 0.93);z-index:999;" aria-hidden="true"></i></a>').appendTo(cameform);
        };

        $('#LvBatchProc').on('click', function () {
            var htmltag = "<span>Are you sure want to process..?</span>";
            var newDiv = $(document.createElement('div'));
            newDiv.html(htmltag);
            newDiv.dialog({
                autoOpen: false,
                title: "Confirmation",
                height: 250, width: 400, modal: true,
                buttons: {
                    Yes: function (e) {
                        ajaxloaderv2('body');
                        var formjson = [];
                        var tr = [];
                        tr = $('#link-popup-approval-lvnewreq tr:gt(0)').toArray();
                        if (tr.length == 0) {
                            tr = $('#link-popup-approval-lvnewreq tr:gt(0)').toArray();
                        }

                        $.each(tr, function (i, k) {
                            var childs = $(k).children().toArray();
                            formjson.push({
                                "Emp": $(childs[0]).text(),
                                "ReqDate": $(childs[1]).text(),
                                "LvHead": $(childs[2]).text(),
                                "FromDate": $(childs[3]).text(),
                                "Todate": $(childs[4]).text()
                            });
                        });
                        $.ajax({
                            url: "@Url.Action("CreateBatchProc", "ELMS")",
                            method: "POST",
                            cache: false,
                            contentType: 'application/json',
                            datatype: 'json',
                            data: JSON.stringify({ data: formjson }),
                            success: function (data) {
                                // $('#monthTable tr:gt(0)').remove();
                                alert(data.responseText);
                                newDiv.dialog("close");
                                newDiv.remove();
                                $('.ajax_loder').remove();
                                $('#link-popup-approval-lvnewreq').LinkPopup().AjxRefresh();
                            }
                        });
                    }, No: function (e) {
                        newDiv.dialog("close");
                        newDiv.remove();

                    }
                }
            });
            newDiv.dialog('open');
            var dia = $('</div>').dialog({

            });



        });
        $('#hide-intro-class').on('click', function () {
            $('.intro-class').hide();
        });
        $('.rep-icon').on('click', function () {
            var color = $(this).css('color');
            if (color == "rgb(255, 255, 255)") {
                $(this).css({ "color": "rgb(0, 0, 0)" });
            }
            else {
                $(this).css({ "color": "rgb(255, 255, 255)" });
            }
            $('#profile-content').toggle(function () {
                $("#profile-content").css({ "z-index": "0", "position": "relative" });
            }, function () {
                $("#profile-content").css({ "z-index": "999", "position": "absolute" });
            });
        });
        function fun_Property(parent_node, returndata, Model) {
            ;
            var data = returndata[Model];
            // console.log(data);
            if (data == undefined) {
                return null;
            }
            var chart_config = [];

            var Viewer_Node = {
                parent: parent_node,
                text: { name: null, title: null, desc: null }

            };
            var Recommandation_Node = {
                parent: parent_node,
                text: { name: null, title: null, desc: null }

            };

            if (data.Viewer != null) {
                Viewer_Node.text.name = "Viewer";
                Viewer_Node.text.title = data.Viewer.EmpCode || "";
                Viewer_Node.text.desc = data.Viewer.EmpName || "";

                chart_config.push(Viewer_Node);
            }
            if (data.Recommendation != null) {
                Recommandation_Node.text.name = "Recommendation";
                Recommandation_Node.text.title = data.Recommendation.EmpCode || "";
                Recommandation_Node.text.desc = data.Recommendation.EmpName || "";

                chart_config.push(Recommandation_Node);
            }
            if (Model == "ELMS") {

                $.each(returndata.ELMS, function (i, k) {
                    console.log("---ELMS--");
