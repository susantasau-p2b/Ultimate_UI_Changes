@{
    Layout = null;
}
@model P2b.Global.BenefitNominees
<style>

    .qual_popup-content-textbox {
        min-width: 200px;
        margin: 0px 0px 1px 3px;
    }

    .qual_popup-content-label {
        display: block;
        float: left;
        width: 165px;
    }

    .content_margine_quali {
        margin: 2.5px;
        padding: 0.5px;
    }

    .qual_row3 {
        width: 473px;
    }

    .qual_icon_row3 {
        left: 118px;
        top: -150px;
        position: relative;
    }

    .popup-content-label {
        width: 100px;
        display: block;
        float: left;
    }

    .row1 {
        width: 635px;
        display: inline-block;
    }

    .hide_textbox {
        display: none;
    }

    .container_div {
        border: 1px solid rgba(207, 207, 207, 0.91);
        margin-top: 15px;
        margin-bottom: 15px;
    }

    .section1_Leave {
        display: block;
        border: 1.5px solid rgba(207, 207, 207, 0.91);
        height: 554px;
        width: 534px;
        margin: 4px auto;
        position: absolute;
    }

    .section2_Leave {
        display: block;
        border: 1px solid #ddd;
        height: 554px;
        width: 610px;
        margin: 5px 4px;
        position: absolute;
        left: 546px;
    }

    .heading {
        background: rgba(207, 207, 207, 0.91) none repeat scroll 0 0;
        color: #424242;
        display: block;
        font-size: 1em;
        font-weight: bold;
        height: 15px;
        margin: 3px auto;
        padding: 2px 0 1px 2px;
        width: 99%;
    }

    .Sanction-div {
        display: none;
    }

    .lv-label {
        width: 121px;
        font-size: 9px;
        padding: 1px;
        position: relative;
        float: left;
    }

    .lb-data {
        font-weight: bold;
        font-size: 11px;
        float: left;
    }

</style>
<form method="post" id="frmBenefitNomineesView" name="frmBenefitNomineesView">
    <input type="text" class="hide_textbox" name="maintableid" id="maintableid" />
    <div id="create_dialog" title="Create PF And Gratuity Nominal Details">
        <div class="section_Method" id="lv-div" style="display:block">
            <div class="heading">Employee Info </div>
            <div class="row1 content_margine">
                <label class="lv-label">Employee :</label>
                <div id="Employee" class="lb-data"></div>
            </div>
        </div>
        <div class="heading">PF And Gratuity Nominal Details </div>
        <div class="row1 content_margine">
            <label class="lv-label">Nominee Name :</label>
            <div id="NomineeName" class="lb-data"></div>
        </div>
        <div class="row1 content_margine">
            <label class="lv-label">Relation :</label>
            <div id="Relation" class="lb-data"></div>
        </div>
        <div class="row1 content_margine">
            <label class="lv-label">Date of Birth :</label>
            <div id="DateOfBirth" class="lb-data"></div>
        </div>
        <div class="row1 content_margine">
            <label class="lv-label">benefit perc :</label>
            <div id="benfperc" class="lb-data"></div>
        </div>
        <div class="row1 content_margine">
            <label class="lv-label">benefit type :</label>
            <div id="benftype" class="lb-data"></div>
        </div>
        <div class="row1 content_margine">
            <label class="lv-label">Address :</label>
            <div id="Address" class="lb-data"></div>
        </div>
        <div class="row1 content_margine">
            <label class="lv-label">Contact Details :</label>
            <div id="ContactDetails" class="lb-data"></div>
        </div>
        
    </div>
    <div class="section_Method" id="Sanction-div">
        <div id="SanctionDiv">
            <div class="heading">Approval Authority Views</div>
            <div id="yes_no_div" class="row1 content_margine">
                <label class="popup-content-label">
                    Approval Status:
                </label>
                <div class="popup_radio_button">
                    <input type="radio" name="Approval" value="true" id="rdo1" /><label for="rdo1">Yes</label>
                    <input type="radio" name="Approval" id="rdo2" value="false" /><label for="rdo2">No</label>
                </div>
            </div>
            <div class="row1 content_margine">
                <label class="popup-content-label">Reason :</label>
                <input type="text" id="Grid-ReasonApproval" name="ReasonApproval" />
            </div>
        </div>
    </div>
</form>
<script>
    $(function () {
        $('button').button();
        $('.drop').selectmenu().selectmenu('menuWidget');
        $('.date').P2BDatePicker();
        $('.popup_radio_button').buttonset();
    });
</script>