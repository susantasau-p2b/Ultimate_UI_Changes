<script>
    $(function () {
        $('button').button();
        $('#DateofBirth').P2BDatePicker();

        $('#lookup-Address-familyDetails').on('click', function (event) {
            $('.dialog').P2BLookUpModal('@Url.Action("GetAddressLKDetails", "familyDetails")', '', 'lookupdiv', 'Address Look Up Data', '#Addresslist', 'Addresslist', 5, 'Addresslist', '#frmFamilyDetails', 'N', '#edit-Address-familyDetails,#remove-Address-familyDetails', 'lookup_table', 'pageNavPosition');
            event.preventDefault();
        });

        // PArtial page create
        $('<div></div>').P2BPartialCreateModalDialog();
        $('#create-Address-familyDetails').on("click", function (event) {
            $('<div></div>').P2BPartialCreateModalDialog('@Url.Action("partial", "address")', 'Create Address', 'open', '@Url.Action("Create","Address")', '#frmAddress', 'Message :', 'Request Falied :', '#jqgrid', 400, 500, '', '', '', '#frmFamilyDetails', '', function (data) {
                // to show data which is created in partial page
                $('#Addresslist').P2BLookUpEncapsulate('#Addresslist', 'Addresslist', data[0], data[1], 'Addresslist', '#edit-Address-familyDetails,#remove-Address-familyDetails', 'N');
            });
            event.preventDefault();
        });

        // Partial page edit
        $('input[name="IShandicap"]:radio').change(function () {
            if (this.value == "false") {
                $('#HandicapRemark').removeClass('must');
                $("#handicapRemarkdiv").fadeOut();
            }
            else {
                $("#handicapRemarkdiv").fadeIn();
                $("#HandicapRemark").val("");
            }
        });
        $('#edit-Address-familyDetails').button().on("click", function (event) {
            var a = [], b;
            a = $('#Addresslist').P2BGetTableDataonSelectedRow('#Addresslist', 0);
            b = a[0];
            //url, openurl, opendataforward, editurl, maindialogtitle, forwardserializedata, forwarddata, editmessage, editerrormessage, gridreloadname, height, width, classoridoftheonwhichpopupderived, nameclassidofinlinelookup, nameofthelist_inlinelookuptable, nameoftable_inlinelookuptable,multiple_allowed_or_not,nameidclassofbuttontodisable,returndatafunction) {
            $('<div></div>').P2BPatialEditModalDialog('@Url.Action("partial", "address")', '@Url.Action("edit", "address")', b, '@Url.Action("Editsave","Address")', 'Edit Address', '#frmAddress', b, 'Message :', 'Request Falied :', '#jqgrid', 400, 500, '#frmFamilyDetails', '#Addresslist', 'Addresslist', 'Addresslist', 'N', '#edit-Address-familyDetails,#remove-Address-familyDetails', function (data) {
                // setTimeout(function () {
                //to bind data according to selected id
                $.each(data[0], function (i, k) {
                    $('#frmAddress').trigger("reset");
                    $('#Address1').val(k.Address1);
                    $('#Address2').val(k.Address2);
                    $('#Address3').val(k.Address3);
                    $('#Landmark').val(k.Landmark);
                    $('#CountryList').P2BSelectMenuAppend("@Url.Action("PopulateDropDownList", "country")", "", k.Country_Id, '');
                    $('#StateList').P2BSelectMenuAppend("@Url.Action("PopulateDropDownList", "state")", "", k.State_Id, '');
                    $('#StateRegionList').P2BSelectMenuAppend("@Url.Action("PopulateDropDownList", "StateRegion")", "", k.StateRegion_Id, '');
                    $('#Districtlist').P2BSelectMenuAppend("@Url.Action("PopulateDropDownList", "District")", "", k.District_Id, '');
                    $('#TalukaList').P2BSelectMenuAppend("@Url.Action("PopulateDropDownList", "Taluka")", "", k.Taluka_Id, '');
                    $('#CityList').P2BSelectMenuAppend("@Url.Action("PopulateDropDownList", "City")", "", k.City_Id, '');
                    $('#AreaList').P2BSelectMenuAppend("@Url.Action("PopulateDropDownList", "area")", "", k.Area_Id, '');
                });
                //}, 3000);


            });
            event.preventDefault();
        });

        //partial page delete
        $('#remove-Address-familyDetails').button().on('click', function (event) {
            //dt = $('#jqgrid').P2BGrid.P2BEdDelV('#jqgrid', 'Id');
            var a = [], b;
            a = $('#Addresslist').P2BGetTableDataonSelectedRow('#Addresslist', 0);
            b = a[0];
            $('<div></div>').P2BPartialDeleteModalDialog('', b, 0, 'Are you sure want to delete record???', 'Message : ', 'Request failed', '#Addresslist', $('#Addresslist').val(), 200, 200, '#frmFamilyDetails', '#Addresslist', '#edit-Address-familyDetails,#remove-Address-familyDetails');
            event.preventDefault();
        });

        /* ContactDetails button click*/

        $('#lookup-ContactDetails-familyDetails').button().on('click', function (event) {
            @*$('.dialog').P2BLookUpModal('@Url.Action("GetAddressLKDetails", "familyDetails")', '', 'lookupdiv', 'Address Look Up Data', '#Addresslist', 'Addresslist', 5, 'Addresslist', '#frmFamilyDetails', 'N', '#edit-Address-familyDetails,#remove-Address-familyDetails', 'lookup_table', 'pageNavPosition');*@
            $('.dialog').P2BLookUpModal('@Url.Action("GetContactDetLKDetails", "familyDetails")', '', 'lookupdiv', 'ContactDetails Look Up Data', '#ContactDetailslist', 'ContactDetailslist', 5, 'ContactDetailslist', '#frmFamilyDetails', 'N', '#edit-ContactDetails-familyDetails,#remove-ContactDetails-familyDetails', 'lookup_table', 'pageNavPosition');
            // $('.ContactDetLKDiv').P2BLookUpModal('@Url.Action("GetContactDetLKDetails", "familyDetails")', '', 'ContactDetLKDiv', 'Look Up Data', '#ContactDetailslist', 'ContactDetailslist', 10, 'ContactDetailslist', '#frmFamilyDetails', 'N', '#edit-ContactDetails-familyDetails,#remove-ContactDetails-familyDetails', 'lookupdiv_ContactDet', 'pageNavPosition_ContactDet');
            event.preventDefault();
        });

        $('<div></div>').P2BPartialCreateModalDialog();
        $('#create-ContactDetails-familyDetails').button().on("click", function (event) {
            //function (url, maindialogtitle, state, submiturl, submitnameformforserilize, savemessage, errormessage, gridreloadname, height, width, forwarddata, ControlName, event, classoridoftheonwhichpopupderived, nameidclassofbuttontodisable, returnfunctiondata) {
            $('<div></div>').P2BPartialCreateModalDialog('@Url.Action("Createcontactdetails_partial", "familyDetails")', 'Create ContactDetails', 'open', '@Url.Action("CreateSave","ContactDetails")', '#frmContactDetails', 'Message :', 'Request Falied :', '#jqgrid', 400, 500, '', '', '', '#frmContactDetails', '#edit-ContactNo-ContDetails,#remove-ContactNo-ContDetails', function (data) {
                $('#ContactDetailslist').P2BLookUpEncapsulate('#ContactDetailslist', 'ContactDetailslist', data[0], data[1], 'ContactDetailslist', '#edit-ContactDetails-familyDetails,#remove-ContactDetails-familyDetails', 'N');
            });
            event.preventDefault();
        });

        $('#edit-ContactDetails-familyDetails').button().on("click", function (event) {
            var a = [], b;
            a = $('#ContactDetailslist').P2BGetTableDataonSelectedRow('#ContactDetailslist', 0);
            b = a[0];
            console.log(b);
            //url, openurl, opendataforward, editurl, maindialogtitle, forwardserializedata, forwarddata, editmessage, editerrormessage, gridreloadname,                                                                                                                    height, width, classoridoftheonwhichpopupderived, nameidclassofbuttontodisable, nameclassidofinlinelookup, nameofthelist_inlinelookuptable, nameoftable_inlinelookuptable,multiple_allowed_or_not, returndatafunction) {
            $('<div></div>').P2BPatialEditModalDialog('@Url.Action("Createcontactdetails_partial", "familyDetails")', '@Url.Action("Editcontactdetails_partial", "familyDetails")', b, '@Url.Action("EditSave", "ContactDetails")', 'Edit Contact Details', '#frmContactDetails', b, 'Message :', 'Request Falied :', '#jqgrid', 400, 500, '#frmFamilyDetails', '#ContactDetailslist', 'ContactDetailslist', 'ContactDetailslist', 'N', '#remove-ContactNo-ContDetails,#edit-ContactNo-ContDetails', function (data) {
                // setTimeout(function () {
                $.each(data[0], function (i, k) {
                    $('#frmContactDetails').trigger('reset');
                    $('#EmailId').val(k.EmailId);
                    $('#Website').val(k.Website);
                    $('#FaxNo').val(k.FaxNo);
                });

                $.each(data[1], function (i, k) {
                    $('#ContactNos_List').P2BLookUpEncapsulate('#ContactNos_List', 'ContactNos_List', k.Id, k.FullContactNumbers, 'ContactNos_List', '#edit-ContactNo-ContDetails,#remove-ContactNo-ContDetails', 'A');
                });
                // }, 3000);
            });
            event.preventDefault();
        });

        $('#remove-ContactDetails-familyDetails').button().on('click', function (event) {
            // dt = $('#jqgrid').P2BGrid.P2BEdDelV('#jqgrid', 'Id');
            var a = [], b;
            a = $('#ContactDetailslist').P2BGetTableDataonSelectedRow('#ContactDetailslist', 0);
            b = a[0];
            $('<div></div>').P2BPartialDeleteModalDialog('@Url.Action("DeleteContactDetails","familyDetails")', b, 0, 'Are you sure want to delete record???', 'Message : ', 'Request failed', '#ContactDetailslist', $('#ContactDetailslist').val(), 200, 200, '#frmFamilyDetails', '#ContactDetailslist', '#edit-ContactDetails-familyDetails,#remove-ContactDetails-familyDetails');
            event.preventDefault();
        });





        $('#lookup-MemberName-familyDetails').button().on('click', function (event) {
            $('.dialog').P2BLookUpModal('@Url.Action("GetLookupNomineesName", "familyDetails")', '', 'lookupdiv', ' Look Up Data', '#MemberNameId', 'MemberNameId', 5, 'MemberNameId', '#frmFamilyDetails', 'N', '#edit-MemberName-familyDetails,#remove-MemberName-familyDetails', 'lookup_table', 'pageNavPosition');
            event.preventDefault();
        });



        $('<div></div>').P2BPartialCreateModalDialog();
        $('#create-MemberName-familyDetails').on("click", function (event) {
            $('<div></div>').P2BPartialCreateModalDialog('@Url.Action("partial", "NameSingle")', 'Create NomineeName', 'open', '@Url.Action("Create", "NameSingle")', '#frmNameSingle', 'Message :', 'Request Falied :', '#jqgrid', 400, 500, '', '', '', '#frmFamilyDetails', '', function (data) {
                // to show data which is created in partial page
                $('#MemberNameId').P2BLookUpEncapsulate('#Namelist', 'Namelist', data[0], data[1], 'Namelist', '#edit-MemberName-familyDetails,#remove-MemberName-familyDetails', 'N');
            });
            event.preventDefault();
        });

        $('#edit-MemberName-familyDetails').button().on("click", function (event) {
            var a = [], b;
            a = $('#MemberNameId').P2BGetTableDataonSelectedRow('#MemberNameId', 0);
            b = a[0];
            console.log(b);
            $('<div></div>').P2BPatialEditModalDialog('@Url.Action("partial", "NameSingle")', '@Url.Action("edit", "NameSingle")', b, '@Url.Action("EditSave", "NameSingle")', 'Edit  Details', '#frmNameSingle', b, 'Message :', 'Request Falied :', '#jqgrid', 400, 500, '#frmFamilyDetails', '#edit-MemberName-familyDetails,#remove-MemberName-familyDetails', '#Namelist', 'Namelist', 'Namelist', 'N', function (data) {
                // setTimeout(function () {
                $.each(data[0], function (i, k) {
                    $('#frmNameSingle').trigger('reset');
                    $('#FName').val(k.FName);
                    $('#MName').val(k.MName);
                    $('#LName').val(k.LName);
                    $('#FName').val(data.FName);
                    $('#title').P2BSelectMenuAppend("@Url.Action("GetLookupValue", "lookup")", 100, "");
                });


                // }, 3000);
            });
            event.preventDefault();
        });

        $('#remove-MemberName-familyDetails').button().on('click', function (event) {
            // dt = $('#jqgrid').P2BGrid.P2BEdDelV('#jqgrid', 'Id');
            var a = [], b;
            a = $('#MemberNameId').P2BGetTableDataonSelectedRow('#MemberNameId', 0);
            b = a[0];
            $('<div></div>').P2BPartialDeleteModalDialog('', b, 0, 'Are you sure want to delete record???', 'Message : ', 'Request failed', '#MemberNameId', $('#MemberNameId').val(), 200, 200, '#frmFamilyDetails', '#MemberNameId', '#edit-MemberName-familyDetails,#remove-MemberName-familyDetails');
            event.preventDefault();
        });

        $('.popup_radio_button').buttonset();
        $('.drop').selectmenu().selectmenu("menuWidget");

        $('#Professionlist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 304, ""); // To bind dropdown on page load
        $('#Relationlist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 107, "");
    });
</script>


