@{
    Layout = null;
}
@model Training.TrainingSchedule

<style>
    .icon-row {
        position: relative;
        left: 84px;
        top: -150px;
    }

    #BusinessType-button {
        font-size: 9px;
        padding-top: 1px;
        font-weight: bold;
    }

    .popup-content-label {
        display: block;
        float: left;
        width: 130px;
    }

    .popup-content-textbox {
        margin-bottom: 1px;
        min-width: 150px;
    }

    .row3 {
        width: 626px;
    }

    .row4 {
        position: absolute;
        top: 294px;
        width: 400px;
    }

    .popup-content-textbox-long {
        margin-bottom: 1px;
        width: 290px;
    }


    .row2 {
        width: 402px;
    }

    .section_Method {
        border: 1px solid rgba(207, 207, 207, 0.91);
        border-radius: 10px;
        margin-top: 15px;
        margin-bottom: 15px;
    }

    .process_btn {
        float: right;
    }

    .section2_Leave {
        width: 401px;
    }

    #lvdata {
        display: block;
        float: right;
    }
</style>
<html>

<head>
    <title></title>
    <script>
        $(document).ready(function (e) {

            $('button').button();
            $('.drop').selectmenu().selectmenu("menuWidget");
            $('#Emp').val("@Request.RequestContext.HttpContext.Session["TempEmpId"]");


            ///training session

            $('#create-TrainingSession-TrainingSchedule').on("click", function (event) {
                $('<div></div>').P2BPartialCreateModalDialog('@Url.Action("Partial", "TrainingSession")', 'Create Dialog', 'open', '@Url.Action("CreateSave", "TrainingSession")', '#frmContactNo', 'Message :', 'Request Falied :', '#jqgrid', 200, 300, '', '', '', '#frmTrainingSchedule', '#edit-TrainingSession-TrainingSchedule,#remove-TrainingSession-TrainingSchedule', function (data) {
                    $('#TrainingSession_List').P2BLookUpEncapsulate('#TrainingSession_List', 'TrainingSession_List', data[0], data[1], 'TrainingSession_List', '#edit-TrainingSession-TrainingSchedule,#remove-TrainingSession-TrainingSchedule', 'N');
                });
                event.preventDefault();
            });
            $('#lookup-TrainingSession-TrainingSchedule').on('click', function (event) {
                $('.dialog').P2BLookUpModal('@Url.Action("Get_TrainingSessionLookupValue", "TrainingFeedback")', '', 'lookupdiv', 'TrainingSession Look Up Data', '#TrainingSession_List', 'TrainingSession_List', 5, 'TrainingSession_List', '#frmTrainingSchedule', 'A', '#remove-TrainingSession-TrainingSchedule', 'lookup_table', 'pageNavPosition');
                event.preventDefault();
            });

            @*$('#edit-TrainingSession-TrainingSchedule').button().on("click", function (event) {
                var a = [], b;
                a = $('#TrainingSession_List').P2BGetTableDataonSelectedRow('#TrainingSession_List', 0);
                b = a[0];
                event.preventDefault();
                $('<div></div>').P2BPatialEditModalDialog('@Url.Action("Partial", "TrainingSession")', '@Url.Action("Edit", "TrainingSession")', b, '@Url.Action("EditSave", "TrainingSession")', 'Edit TrainingSession', '#frmTrainingSession', b, 'Message :', 'Request Falied :', '#jqgrid', 200, 300, '#frmTrainingSchedule', '#TrainingSession_List', 'TrainingSession_List', 'TrainingSession_List', 'N', '#edit-TrainingSession-TrainingSchedule,#remove-TrainingSession-TrainingSchedule', function (data) {

                    $.each(data, function (i, k) {
                        $('#frmTrainingSession').trigger('reset');
                        $('#MobileNo').val(k.MobileNo);
                        $('#LandlineNo').val(k.LandlineNo);
                        $('#STDCode').val(k.STD);
                    });
                });

            });

            $('#remove-TrainingSession-TrainingSchedule').button().on('click', function (event) {
                var a = [], b;
                a = $('#TrainingSession_List').P2BGetTableDataonSelectedRow('#TrainingSession_List', 0);
                b = a[0];
                $('<div></div>').P2BPartialDeleteModalDialog('', b, 0, 'Are you sure want to delete record???', 'Message : ', 'Request failed', '#TrainingSession_List', $('#TrainingSession_List').val(), 200, 200, '#frmTrainingSchedule', '#TrainingSession_List', '#remove-TrainingSession-TrainingSchedule');
                event.preventDefault();
            });*@

            ///training feedback

            $('#create-TrainingFeedback-TrainingSchedule').on("click", function (event) {
                $('<div></div>').P2BPartialCreateModalDialog('@Url.Action("Partial1", "TrainingFeedback")', 'Create Dialog', 'open', '@Url.Action("CreateSavePartial", "TrainingFeedback")', '#frmTrainingFeedback', 'Message :', 'Request Falied :', '#jqgrid', 300, 400, '', '', '', '#frmLvNewReq', '#edit-TrainingFeedback-TrainingSchedule,#remove-TrainingFeedback-TrainingSchedule', function (data) {
                    $('#TrainingFeedback_List').P2BLookUpEncapsulate('#TrainingFeedback_List', 'TrainingFeedback_List', data[0], data[1], 'TrainingFeedback_List', '#edit-TrainingFeedback-TrainingSchedule,#remove-TrainingFeedback-TrainingSchedule', 'N');
                });
                event.preventDefault();
            });
            $('#lookup-TrainingFeedback-TrainingSchedule').on('click', function (event) {
                $('.dialog').P2BLookUpModal('@Url.Action("Get_TrainingFeedbackLookupValue", "TrainingFeedback")', '', 'lookupdiv', 'TrainingFeedback Look Up Data', '#TrainingFeedback_List', 'TrainingFeedback_List', 5, 'TrainingFeedback_List', '#frmTrainingSchedule', 'A', '#remove-TrainingFeedback-TrainingSchedule', 'lookup_table', 'pageNavPosition');
                event.preventDefault();
            });

            $('#edit-TrainingFeedback-TrainingSchedule').button().on("click", function (event) {
                var a = [], b;
                a = $('#TrainingFeedback_List').P2BGetTableDataonSelectedRow('#TrainingFeedback_List', 0);
                b = a[0];
                event.preventDefault();
                $('<div></div>').P2BPatialEditModalDialog('@Url.Action("Partial1", "TrainingFeedback")', '@Url.Action("EditPartial", "TrainingFeedback")', b, '@Url.Action("EditSave", "TrainingFeedback")', 'Edit TrainingFeedback', '#frmTrainingFeedback', b, 'Message :', 'Request Falied :', '#jqgrid', 200, 300, '#frmTrainingSchedule', '#TrainingFeedback_List', 'TrainingFeedback_List', 'TrainingFeedback_List', 'N', '#edit-TrainingFeedback-TrainingSchedule,#remove-TrainingFeedback-TrainingSchedule', function (data) {

                    $.each(data, function (i, k) {
                        $('#frmTrainingFeedback').trigger('reset');
                       
                    });
                });

            });

            $('#remove-TrainingFeedback-TrainingSchedule').button().on('click', function (event) {
                var a = [], b;
                a = $('#TrainingFeedback_List').P2BGetTableDataonSelectedRow('#TrainingFeedback_List', 0);
                b = a[0];
                $('<div></div>').P2BPartialDeleteModalDialog('', b, 0, 'Are you sure want to delete record???', 'Message : ', 'Request failed', '#TrainingFeedback_List', $('#TrainingFeedback_List').val(), 200, 200, '#frmTrainingSchedule', '#TrainingFeedback_List', '#remove-TrainingFeedback-TrainingSchedule');
                event.preventDefault();
            });

            $('<div></div>').P2BPartialCreateModalDialog();

            $('.popup_radio_button').buttonset();



        });

    </script>
</head>
<body>
    <div class="dialog">
        <div title="LookUp Data">
            <div class="lookupdiv"></div>
        </div>
        <div id="pageNavPosition">

        </div>
    </div>


    <div id="create_dialog" title="Create Training Feedback">
        @using (Html.BeginForm(null, null, FormMethod.Post, new { Id = "frmTrainingSchedule", name = "frmTrainingSchedule" }))
        {
            <input type="text" id="Emp" name="employee-table" class="filter_structure_id" />
            <div class="section2_Leave">





                <div class="row3 content_margine">
                    @Html.LabelFor(m => m.TrainingSession, "Training Session:", new { @class = "popup-content-label" })
                    <div class="popup-content-table">
                        <table class="lookuptableselected" id="TrainingSession_List">
                            <tr>
                                <th>ID</th>
                                <th>LookUp Selected Description</th>
                            </tr>
                        </table>
                    </div>
                    <div class="icon-row">
                        <button data-p2btooltip="Select TrainingSession" class="popup-content-icon-lookup" id="lookup-TrainingSession-TrainingSchedule"><span class="ui-icon ui-icon-folder-open popup-content-iconmark"></span></button>
                        @*<button data-p2btooltip="Create Contact Numbers" class="popup-content-icon-create" id="create-ContactNo-ContDetails"><span class="ui-icon ui-icon-newwin popup-content-iconmark"></span></button>*@
                        @*<button data-p2btooltip="Edit Contact Numbers" class="popup-content-icon-edit" id="edit-ContactNo-ContDetails"><span class="ui-icon ui-icon-pencil popup-content-iconmark"></span></button>*@
                        <button data-p2btooltip="Delete TrainingSession" class="popup-content-icon-remove" id="remove-TrainingSession-TrainingSchedule"><span class="ui-icon ui-icon-trash popup-content-iconmark"></span></button>
                    </div>
                </div>

                <div class="row3 content_margine">
                    @*@Html.LabelFor(m => m.TrainingSession, "Training Session:", new { @class = "popup-content-label" })*@
                    <label class="popup-content-label">Training Feedback</label>
                    <div class="popup-content-table">
                        <table class="lookuptableselected" id="TrainingFeedback_List">
                            <tr>
                                <th>ID</th>
                                <th>LookUp Selected Description</th>
                            </tr>
                        </table>
                    </div>
                    <div class="icon-row">
                        <button data-p2btooltip="Select TrainingFeedback" class="popup-content-icon-lookup" id="lookup-TrainingFeedback-TrainingSchedule"><span class="ui-icon ui-icon-folder-open popup-content-iconmark"></span></button>
                        <button data-p2btooltip="Create TrainingFeedback" class="popup-content-icon-create" id="create-TrainingFeedback-TrainingSchedule"><span class="ui-icon ui-icon-newwin popup-content-iconmark"></span></button>
                        <button data-p2btooltip="Edit TrainingFeedback" class="popup-content-icon-edit" id="edit-TrainingFeedback-TrainingSchedule"><span class="ui-icon ui-icon-pencil popup-content-iconmark"></span></button>
                        <button data-p2btooltip="Delete TrainingFeedback" class="popup-content-icon-remove" id="remove-TrainingFeedback-TrainingSchedule"><span class="ui-icon ui-icon-trash popup-content-iconmark"></span></button>
                    </div>
                </div>

            </div>
        }
    </div>
</body>
</html>
