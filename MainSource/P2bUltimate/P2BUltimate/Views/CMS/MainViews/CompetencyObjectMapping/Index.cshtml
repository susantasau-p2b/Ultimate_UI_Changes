</body>
</html>
<script>

    $(document).ready(function (e) {
        $('button').button();       
        var urlfilter = "";
        $('#NewModelist').on('selectmenuchange', function () {
             
            var newvalue = $("#NewModelist option:selected").text();
            var urlfilter = "@Url.Action("CompetencyObjectMapping_Grid")?NewModellist=" + newvalue;
            $('#MyTable').MultiLevelInlineEditGrid({
                url: urlfilter,
                editurl: "@Url.Action("EditGridDetails", "DivisionTargetSetting")",
                htmurl: "@Url.Action("partial", "DivisionTargetSetting")",
                submiturl: "@Url.Action("GridEditSave", "DivisionTargetSetting")",
                columnname: ["LookupVal", "LookupValData", "Id", ],
                //childheader: ["Category", "SubCategory", "Accounts", "Amount", "Compliance", "Id"],
                //childheader: ["Id", "LoanAdvHead", "RequisitionDate", "EnforcementDate", "LoanAppliedAmt", "LoanSanctionedAmt", "MonthlyInstallmentAmt", "TotalInstallment", "LoanAccBranch", "LoanAccNo"],
                childurl: "@Url.Action("Get_TargetSettingRequest")",
                tableheader: "ObjectLookupMapping",
                childurlColumnNo: "3"
            });
        });

        var valueable = "--Select--";
        $('#MyTable').MultiLevelInlineEditGrid({
            url: "@Url.Action("CompetencyObjectMapping_Grid")?NewModellist=" + valueable,
            editurl: "@Url.Action("EditGridDetails", "DivisionTargetSetting")",
            htmurl: "@Url.Action("partial", "DivisionTargetSetting")",
            submiturl: "@Url.Action("GridEditSave", "DivisionTargetSetting")",
            columnname: ["LookupVal", "LookupValData", "Id", ],
            //childheader: ["Category", "SubCategory", "Accounts", "Amount", "Compliance", "Id"],
            //childheader: ["Id", "LoanAdvHead", "RequisitionDate", "EnforcementDate", "LoanAppliedAmt", "LoanSanctionedAmt", "MonthlyInstallmentAmt", "TotalInstallment", "LoanAccBranch", "LoanAccNo"],
            @*childurl: "@Url.Action("Get_TargetSettingRequest")",*@
            tableheader: "ObjectLookupMapping",
            childurlColumnNo: "3"
        });

        $.DataTableChildManipulation = function (init, id, parentid, htmurl, editurl, submiturl) {
            var parent_element = $(init).parents('td:last').parent().prev();
            console.log(parent_element);
            var dt = $('#MyTable').DataTable();
            var htmlcomponant = {
                init: init,
                Id: id,
                idx: parentid,
                htmurl: htmurl,
                editurl: editurl,
                submiturl: submiturl,
            };
            if ($(init).hasClass('ui-icon-newwin')) {
                $('<div></div>').PartialCreateDialog({
                    height: 'auto',
                    width: 'auto',
                    form: "#Edit-loanadvrequest",
                    htmlurl: "@Url.Action("GridPartial", "LoanAdvRequest")",
                    state: "open",
                    mode: "View",
                    editurl: "@Url.Action("GridEditData", "LoanAdvRequest")",
                    submiturl: "@Url.Action("GridEditSave", "LoanAdvRequest")",
                    editdata: id,
                    forwarddata: id,
                    returndatafunction: function (returndata) {
                        $.each(returndata.returndata, function (i, k) {
                            $('#LoanAdvanceHead-edit').val(k.LoanAdvanceHead);
                            $('#TotalInstallment-edit').val(k.TotalInstallement);
                            $('[name="IsActive"]').val([k.ISActive]).button().button("refresh");
                            $('#CloserDate-edit').val(k.CloserDate);
                            $('#LoanAccNo-edit').val(k.LoanAccNo);
                            $('#Edit-LoanAccBranch').P2BSelectMenuAppend("@Url.Action("LoanAccBranchDrop")", "", k.LoanAccBranch);

                        });
                    },
                });
            } else if ($(init).hasClass('ui-icon-pencil')) {
                // //debugger
                $('<div></div>').PartialCreateDialog({
                    height: 'auto',
                    width: '35%',
                    form: "#Edit-loanadvrequest",
                    htmlurl: "@Url.Action("GridPartial", "LoanAdvRequest")",
                    state: "open",
                    mode: "Edit",
                    editurl: "@Url.Action("GridEditData", "LoanAdvRequest")",
                    submiturl: "@Url.Action("GridEditSave", "LoanAdvRequest")",
                    editdata: id,
                    forwarddata: id,
                    returndatafunction: function (returndata) {
                        $.each(returndata.returndata, function (i, k) {
                            $('#LoanAdvanceHead-edit').val(k.LoanAdvanceHead);
                            $('#TotalInstallment-edit').val(k.TotalInstallement);
                            $('[name="IsActive"]').val([k.ISActive]).button().button("refresh");
                            $('#CloserDate-edit').val(k.CloserDate);
                            $('#LoanAccNo-edit').val(k.LoanAccNo);
                            $('#Edit-LoanAccBranch').P2BSelectMenuAppend("@Url.Action("LoanAccBranchDrop")", "", k.LoanAccBranch);

                        });
                    },
                    submitfun: function (data) {
                        var row = dt.row(parent_element);
                        if (row.child.isShown()) {
                            parent_element.removeClass('details');
                            row.child.hide();
                        }
                    }
                });
            } else if ($(init).hasClass('ui-icon-trash')) {
                $('<div></div>').PartialCreateDialog({
                    state: "open",
                    mode: "Delete",
                    submiturl: "@Url.Action("Delete")",
                    forwarddata: id,
                    submitfun: function (data) {
                        var row = dt.row(parent_element);
                        if (row.child.isShown()) {
                            parent_element.removeClass('details');
                            row.child.hide();
                        }
                    }
                });
            }
        };

        function AssignData() {
            dt = $('#JqGrid').P2BGrid.P2BEdDelV('#JqGrid', 'Id');
        }


        $('#Create_Dialog').P2BCreateDialog();
        $('#Create').on("click", function (event) {
            $('#frmObjectLookupMapping').trigger("reset");
            $('#Create_Dialog').P2BCreateDialog('', '', '', '', 'ObjectLookupMapping', 'open', '@Url.Action("EditSave")', '#frmCompetencyObjectMapping', 'Message :', 'Request Falied :', '#JqGrid', 570, 1140, '', '', function (data) { }, {});
            $('#AppModelist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 1069, "");
            event.preventDefault();
        });

        $('#AppModelist').on('selectmenuchange', function () {
            var value = $("#AppModelist option:selected").text();
            $('#OBJMIDList').P2BSelectMenuAppend('@Url.Action("GetModelList", "CompetencyObjectMapping")?Apprlist=' + value, '', "");
        });

        $('#OBJMIDList').on('selectmenuchange', function () {
            var value = $("#AppModelist option:selected").text();
            var value1 = $("#OBJMIDList option:selected").text();
            $('#MAPPIDList').P2BSelectMenuAppend('@Url.Action("GetModelObjList", "CompetencyObjectMapping")?Apprlist=' + value + '&Objmlist=' + value1, '', "");
        });

        $('#Lookup-AppCategoryList').button().on('click', function (event) {
            var value = $("#AppModelist option:selected").val();
            $('.dialog').P2BLookUpModal('@Url.Action("GetAppCategory")?Apprlist=' + value, '', 'LookupDiv', 'Look Up Data', '#AppCategoryList', 'AppCategoryList', 5, 'AppCategoryList', '#frmCompetencyObjectMapping', 'N', '#Remove-Category-DivisionTargetSetting', 'Lookup_table', 'PageNavPosition');
            event.preventDefault();
        });

        $('#Remove-AppCategoryList').button().on('click', function (event) {

            a = $('#AppCategoryList').P2BGetTableDataonSelectedRow('#AppCategoryList', 0);
            b = a[0];
            $('<div></div>').P2BPartialDeleteModalDialog('', b, '', 'Are you sure want to delete record???', 'Message : ', 'Request failed', '#AppCategoryList', $('#AppCategoryList').val(), 200, 200, '#frmCompetencyObjectMapping', '#AppCategoryList');
            event.preventDefault();
        });

        $('.popup_radio_button').controlgroup();
        $('#AppModelist').selectmenu().selectmenu("menuWidget");
        $('#OBJMIDList').selectmenu().selectmenu("menuWidget");
        $('#MAPPIDList').selectmenu().selectmenu("menuWidget");
        $('#NewModelist').selectmenu().selectmenu("menuWidget");
        $('#StartPeriod,#EndPeriod').P2BDatePicker();
    });
</script>
