    <div id="Create_Dialog">
        @using (Html.BeginForm(null, null, FormMethod.Post, new { Id = "frmCompetencyObjectMapping", name = "frmCompetencyObjectMapping" }))
        {
            <input id="Autho_Action" type="hidden" name="Autho_Action" />
            <input id="Autho_Allow" type="hidden" name="Autho_Allow" />
            <input id="Division_id" type="hidden" name="Division_id" />

            <div class="section1_Leave">
                <div class="row3 content_margine">
                    <select class="drop must" style="width:180px;" name="AppModelist" id="AppModelist">
                        <option></option>
                    </select>
                </div>
                <div class="row3 content_margine">
                    <div class="row3 content_margine ">
                        <label for="ModelObjectList" class="popup-content-label">AppCategoryList</label><br>
                        <div class="popup-content-table lookupbox">
                            <table class="lookuptableselected" id="AppCategoryList">
                                <tr><th>Id</th><th>LookUp Selected Description</th></tr>
                            </table>
                        </div>
                        <div class="icon-row">
                            <button data-p2btooltip="Lookup AppCategoryList" class="popup-content-icon-lookup " id="Lookup-AppCategoryList"><span class="ui-icon ui-icon-folder-open popup-content-iconmark "></span></button>
                            <button data-p2btooltip="Remove AppCategoryList" class="popup-content-icon-remove " id="Remove-AppCategoryList"><span class="ui-icon ui-icon-trash popup-content-iconmark"></span></button>
                        </div>
                    </div>

                </div>

            </div>
            <div class="section2_Leave">
                <div class="row3 content_margine">
