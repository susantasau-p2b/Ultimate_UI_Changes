        function AssignData() {
            dt = $('#JqGrid').P2BGrid.P2BEdDelV('#JqGrid', 'Id');
        }
        $('#Create_Dialog').P2BCreateDialog();
        //Create Function
        $('#Create').on("click", function (event) {
            $.FormReset('#FormPastExperienceModel');
            $('#Create_Dialog').P2BCreateDialog('', '', '', '', 'Appraisal Past Experience Model Object', 'open', '@Url.Action("Create")', '#FormPastExperienceModel', 'Message :', 'Request Falied :', '#JqGrid', 520, 520, '', "", function (data) { }, { datatablename: "#MyTable" });
            $('#CreatedDate').TodayDate();
            $('#CreatedDate').attr("readonly", "readonly");
            event.preventDefault();
        });
        $('#Create-PastExperienceModelObject').on("click", function (event) {
            $('<div></div>').P2BPartialCreateModalDialog('@Url.Action("PastExperienceModelPartial", "PastExperienceModel")', 'Past Experience Model Object', 'open', '@Url.Action("Create", "PastExperienceModelObject")', '#FormPastExperienceModelObject', 'Message :', 'Request Falied :', '#JqGrid', 300, 550, '', '', '', '#FormPastExperienceModel', '', function (data) {
                @*$('#PastExperienceModellist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 505, "");*@
                $('#PastExperienceModellist').P2BSelectMenuAppend('@Url.Action("PopulateDropDownList", "PastExperienceModelObject")', '', "");
                $('#PastExperienceModelObjectList').P2BLookUpEncapsulate('#PastExperienceModelObjectList', 'PastExperienceModelObjectList', data[0], data[1], 'PastExperienceModelObjectList', '#Edit-WOCalendar-Location,#Remove-WOCalendar-Location', 'A');
            });
            event.preventDefault();
        });

         $('#Edit').on('click', function () {
             $('#FormPastExperienceModel').trigger("reset");
            var b = localStorage.getItem('MyTable_selected');
            var a = JSON.parse(b);
            $('#Create_Dialog').P2BEditModalDialog('@Url.Action("Edit")', a.Id, '@Url.Action("EditSave")', "Edit Appraisal Past Experience Model Object", '#FormPastExperienceModel', a.Id, 'Message : ', 'Request Falied :', '#JqGrid', 500, 620, '', '', function (data) {
                $.each(data[0], function (i, k) {
                    $('#FormPastExperienceModel').trigger("reset");
                    $('#Code').val(k.Code);
                    $('#ModelName').val(k.ModelName);
                    $('#ModelDescription').val(k.ModelDescription);
                    $('#CreatedDate').val(k.CreatedDate);
                    $('#CreatedDate').attr("readonly", "readonly");
                });

                //partial page bind
                $.each(data[1], function (i, k) {
                    $('#PastExperienceModelObjectList').P2BLookUpEncapsulate('#PastExperienceModelObjectList', 'PastExperienceModelObjectList', k.PastExperienceModelObject_Id, k.PastExperienceModelObject_FullDetails, 'PastExperienceModelObjectList', '#Edit-HOCalendar-Department,#Remove-HOCalendar-Department', 'A');
                });
            }, { datatablename: "#MyTable" });

         });
        $('#View').on('click', function () {
            $('#FormPastExperienceModel').trigger("reset");
            var b = localStorage.getItem('MyTable_selected');
            var a = JSON.parse(b);
            $('#Create_Dialog').P2BViewModalDialog('@Url.Action("Edit")', a.Id, 'View Appraisal Past Experience Model Object', '#PastExperienceModelObjectList', '', 455, 590, '#Autho', '@Url.Action("AuthSave")', a.Id, '', '', '', '', '#JqGrid', function (data) {
                $.each(data[0], function (i, k) {
                    $('#FormPastExperienceModel').trigger("reset");
                    $('#Code').val(k.Code);
                    $('#ModelName').val(k.ModelName);
                    $('#ModelDescription').val(k.ModelDescription);
                    $('#CreatedDate').val(k.CreatedDate);
                    $('#CreatedDate').attr("readonly", "readonly");
                });

                //partial page bind
                $.each(data[1], function (i, k) {
                    $('#PastExperienceModelObjectList').P2BLookUpEncapsulate('#PastExperienceModelObjectList', 'PastExperienceModelObjectList', k.PastExperienceModelObject_Id, k.PastExperienceModelObject_FullDetails, 'PastExperienceModelObjectList', 'view', 'A');
                });
            }, { datatablename: "#MyTable" });

        });

        $('#Lookup-PastExperienceModelObject').on('click', function (event) {
            $('.dialog').P2BLookUpModal('@Url.Action("GetLookupPastExperienceModelObject", "PastExperienceModel")', '', 'LookupDiv', ' PastExperienceModel Look Up Data', '#PastExperienceModelObjectList', 'PastExperienceModelObjectList', 5, 'PastExperienceModelObjectList', '#FormPastExperienceModel', 'A', '#Remove-Witness-Witness', 'Lookup_table', 'PageNavPosition');
            event.preventDefault();
        });
        $('#Edit-PastExperienceModelObject').button().on("click", function (event) {
            var a = [], b;
            a = $('#PastExperienceModelList').P2BGetTableDataonSelectedRow('#PastExperienceModelObjectList', 0);
            b = a[0];
            $('<div></div>').P2BPatialEditModalDialog('@Url.Action("PastExperienceModelPartial", "PastExperienceModel")', '@Url.Action("Edit", "PastExperienceModelObject")', b, '@Url.Action("Editsave", "PastExperienceModelObject")', 'Edit Past Experience Model Object', '#FormPastExperienceModelObject', b, 'Message :', 'Request Falied :', '#JqGrid', 400, 560, '#FormPastExperienceModel', '#PastExperienceModelObjectList', 'PastExperienceModelObjectList', 'PastExperienceModelObjectList', 'N', '', function (data) {

                $.each(data[0], function (i, k) {
                    @*$('#PastExperienceModellist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 505, k.PastExperienceModel_Id);*@
                    $('#PastExperienceModellist').P2BSelectMenuAppend("@Url.Action("PopulateDropDownList", "PastExperienceModelObject")", "", k.PastExperienceModelObject_Id, '');
                    $('#CompetencyEvaluationModelList').P2BLookUpEncapsulate('#CompetencyEvaluationModelList', 'CompetencyEvaluationModelList', k.CompetencyEvaluationModel_Id, k.CompetencyEvaluationModelDetails, 'CompetencyEvaluationModelList', '#Remove-ContactDetails-Location,#Edit-ContactDetails-Location', 'N');
                });
            });
            event.preventDefault();
        });
        $('#Delete').button().click(function (event) {
            $.FormReset('#FormPastExperienceModel');
            var b = localStorage.getItem('MyTable_selected');
            var a = JSON.parse(b);
            $('#Create_Dialog').P2BDeleteModalDialog('@Url.Action("Delete")', a.Id, 'Are you sure want to remove record???', 'Message : ', 'Request failed', '#JqGrid', 200, 200, { datatablename: "#MyTable" });
            event.preventDefault();
        });
        $('#Remove-PastExperienceModelObject').button().on('click', function (event) {
            //var a = [], b;
            //a = $('#PastExperienceModelObjectList').P2BGetTableDataonSelectedRow('#PastExperienceModelObjectList', 0);
            //b = a[0];
            var selectedRows = [];
            selectedRows = $('#PastExperienceModelObjectList').P2BGetTableDataonSelectedRow('#PastExperienceModelObjectList', 0);
            $('<div></div>').P2BPartialDeleteModalDialog('', selectedRows, 0, 'Are you sure want to delete record???', 'Message : ', 'Request failed', '#PastExperienceModelList', $('#PastExperienceModelObjectList').val(), 200, 200, '#FormPastExperienceModel', '#PastExperienceModelObjectList');
            event.preventDefault();
        });

        $('.popup_radio_button').controlgroup();
    });
</script>