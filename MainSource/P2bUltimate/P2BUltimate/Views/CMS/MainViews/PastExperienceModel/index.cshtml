@{
    ViewBag.Title = "PastExperienceModel";
}
@model CMS_SPS.PastExperienceModel
<html>
<head>
    <title></title>
    <style>
        .popup-content-label {
            width: 170px;
            display: block;
            float: left;
        }

        .popup-content-textbox {
            width: 120px;
        }

        .row1 {
            width: 320px;
        }

        .horizontal_row3, .radio_button_div_1 {
            display: inline-block;
        }

        .row3 {
            width: 477px;
        }

        .row2 {
            width: 402px;
        }

        .icon-row {
            left: 127px;
            position: relative;
            top: -121px;
        }

        .popup-content-table {
            right: 120px;
        }
    </style>
</head>
<body>
    <div class="Form_Bg">
        <div class="Form_Input">
            <div class="Button_Container">
                <button class="button" id="Create">Create</button>
                <button class="button" id="Edit">Edit</button>
                <button class="button" id="View">View</button>
                <button class="button" id="Delete">Delete</button>
            </div>
        </div>
    </div>

    <div id="Grid_Form">
        <div>
            <div id="Container" class="container">
                <div id="Demo">
                    <table id="MyTable" class="display" cellspacing="30" width="1000">
                        <thead>
                            <tr>
                                <th>DD</th>
                                <th>ModelCode</th>
                                <th>ModelName</th>
                                <th>ModelDescription</th>
                                <th>CreatedDate</th>
                                <th>ID</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>

    <div class="dialog">
        <div title="LookUp Data">
            <div class="LookupDiv"></div>
        </div>
        <div id="PageNavPosition">

        </div>
    </div>
    <div id="Create_Dialog">
        @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "FormPastExperienceModel", name = "FormPastExperienceModel" }))
        {
            <div class=" row3 content_margine">
                @Html.LabelFor(m => m.Code, "Model Code :", new { @class = "popup-content-label must", @maxlength = "10" })
                @Html.TextBoxFor(m => m.Code, new { @class = "popup-content-textbox must" })
            </div>
            <div class=" row3 content_margine">
                @Html.LabelFor(m => m.ModelName, "Model Name :", new { @class = "popup-content-label must" })
                @Html.TextBoxFor(m => m.ModelName, new { @class = "popup-content-textbox must", @maxlength = "50" })
            </div>
            <div class=" row3 content_margine">
                @Html.LabelFor(m => m.ModelDescription, "Model Description :", new { @class = "popup-content-label must" })
                @Html.TextBoxFor(m => m.ModelDescription, new { @class = "popup-content-textbox must" })
            </div>
            <div class=" row3 content_margine">
                @Html.LabelFor(m => m.CreatedDate, "Created Date :", new { @class = "popup-content-label" })
                @Html.TextBoxFor(m => m.CreatedDate, new { @class = "popup-content-textbox must" })
            </div>
            <div class=" row3 content_margine">
                @Html.LabelFor(m => m.PastExperienceModelObject, "Past Experience Object :", new { @class = "popup-content-label must " })
                <div class="popup-content-table">
                    <table class="lookuptableselected" id="PastExperienceModelObjectList">
                        <tr><th>ID</th><th>LookUp Selected Description</th></tr>
                    </table>
                </div>
            </div>
            <div class="icon-row">
                <button data-p2btooltip="Lookup PastExperienceModelObject" class=" popup-content-icon-lookup" id="Lookup-PastExperienceModelObject"><span class="ui-icon ui-icon-folder-open popup-content-iconmark"></span></button>
                <button data-p2btooltip="Create PastExperienceModelObject " class=" popup-content-icon-create" id="Create-PastExperienceModelObject"><span class="ui-icon ui-icon-newwin popup-content-iconmark"></span></button>
                <button data-p2btooltip="Edit  PastExperienceModelObject" class=" popup-content-icon-edit" id="Edit-PastExperienceModelObject"><span class="ui-icon ui-icon-pencil popup-content-iconmark"></span></button>
                <button data-p2btooltip="Delete PastExperienceModelObject" class=" popup-content-icon-remove" id="Remove-PastExperienceModelObject"><span class="ui-icon ui-icon-trash popup-content-iconmark"></span></button>
            </div>
        }
    </div>
</body>
</html>
<script>
    $(document).ready(function (e) {
        $('button').button();
        $('#Code').ValidateTextBox({ maxlength: 10 });
        $('#ModelName').ValidateTextBox({ maxlength: 25 });
        $('#ModelDescription').ValidateTextBox({ maxlength: 50 });
        $('#MyTable').MultiLevelInlineEditGrid({
            url: "@Url.Action("PastExperienceModel_Grid")",
            columnname: ["Code", "ModelName", "ModelDescription", "CreatedDate", "Id", ],
            childheader: ["PastExperienceModel", "EvaluationModel", "Id"],
            childurl: "@Url.Action("A_PastExperienceModel_Grid", "PastExperienceModel")",
            tableheader: "Past Experience Model",
            childurlColumnNo: "5"
        });

