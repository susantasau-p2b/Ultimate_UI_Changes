                                var OwnerCloseBal = $('#OwnerCloseBal').val();
                                var VPFCloseBal = $('#VPFCloseBal').val();
                                var OwnIntCloseBal = $('#OwnIntCloseBal').val();
                                var OwnerIntCloseBal = $('#OwnerIntCloseBal').val();
                                var VPFIntCloseBal = $('#VPFIntCloseBal').val();
