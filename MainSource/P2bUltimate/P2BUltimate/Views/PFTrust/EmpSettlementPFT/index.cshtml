                        url: "@Url.Action("Process")",
                        data: form,
                        dataType: 'json',
                        success: function (data,status,xhr) {
                            ////debugger;
                          
                          //  alert(data);
                           // alert(data);
                            if (data.success == true) {
                                alert(data.responseText);
                            }
                            
                            //if (data.errorno > 0) {
                            //    alert(data.ShowMessage);
                                //}
                            //if (xhr.status == 200)
                            //{
                              
                            //}
