@{
    ViewBag.Title = "PasswordPolicy";
}
@model P2b.Global.PasswordPolicy
<script>

    $(function () {
        $('button').button();

        var dt;
        var ColNames = [];
        var ColModel = [];
        var SortName, Caption, url, width, height;
        ColModel = ['MinPwdLength', 'MinUpChars', 'MinLowChars', 'MinSymbols', 'AllowEncryption', 'Application', 'Id'];
        ColNames = ['MinPwdLength', 'MinUpChars', 'MinLowChars', 'MinSymbols', 'AllowEncryption', 'Application', 'Id'];
        SortName = 'Id';
        Caption = 'Password Policy';
        url = '@Url.Action("P2BGrid")';
        width = 1000;
        height = 150;

        $('#JqGrid').P2BGrid(ColNames, ColModel, SortName, Caption, url, width, height);

        $('#MinUpChars,#MinLowChars,#ForChangedPwd,#AllowAttempt,#BadLogin,#MinPwdLength,#MinNoNos,#Pwd,#LockMin,#MinAllowRep,#OldPwdAllowCount,#OTPActiveTime,#OTPOnEmailAppl,#OTPRetainDays,#Sessions,#SecurityExpiryTimeSec,#SecurityRetryCount,#PwdChangedAfterDays').ForceOnlyNumeric();


        $("#defult_selection").on("click", function () {
            if ($("#defult_selection").is(":checked")) {
                default_val();
            }
            else { ClearSelection(); }
        });

        function default_val() {
            $('#MinPwdLength').val(8);
            $('#MinUpChars').val(1);
            $('#MinLowChars').val(1);
            $('#MinNoNos').val(1);
            $('#ForChangedPwd').val(3);
            $('#AllowAttempt').val(3);
            $('#BadLogin').val(3);
            $('#MinSymbols').val(1);
            $('[name="Suspend"]').val([true]).button().button("refresh");
            $('[name="MultipleLogin"]').val([true]).button().button("refresh");
            $('[name="OldPwdAllow"]').val([false]).button().button("refresh");
            $('[name="Captcha"]').val([false]).button().button("refresh");
            $('[name="SecurityQuestion"]').val([true]).button().button("refresh");
            $('[name="AllowEncryption"]').val([false]).button().button("refresh");
        };

        function ClearSelection() {
            $('#MinPwdLength').val(0);
            $('#MinUpChars').val(0);
            $('#MinLowChars').val(0);
            $('#MinNoNos').val(0);
            $('#ForChangedPwd').val(0);
            $('#AllowAttempt').val(0);
            $('#BadLogin').val(0);
            $('#MinSymbols').val(0);
            $('[name="Suspend"]').val([false]).button().button("refresh");
            $('[name="MultipleLogin"]').val([false]).button().button("refresh");          
            $('[name="OldPwdAllow"]').val([false]).button().button("refresh");
            $('[name="Captcha"]').val([false]).button().button("refresh");
            $('[name="SecurityQuestion"]').val([false]).button().button("refresh");
            $('[name="AllowEncryption"]').val([false]).button().button("refresh");
        };

        $('#Create_Dialog').P2BCreateDialog();
        $('#Create').button().on("click", function (event) {
            $('#MYdiv').fadeOut();
            $('#MYdiv1').fadeOut();
            $('.all_radio_button').addClass("disable");
            $('#OTPRetainDays,#OldPwdAllowCount,#PwdStregth,#MinLowCharsStrength,#MinUpCharsStrength,#SecurityRetryCount,#SecurityExpiryTimeSec,#OTPOnEmailAppl,#PwdChangedAfterDays,#MinAllowRep,#LockMin,#Pwd,#DisallowedChars,#EmpStatus,#Sessions,#OTPActiveTime').attr('readonly', true);
            $('#frmPasswordPolicy').trigger('reset');
            $('#Create_Dialog').P2BCreateDialog('', 0, '', '', 'Create Password Policy Dialog', 'open', '@Url.Action("Create")', '#frmPasswordPolicy', 'Message :', 'Request Falied :', '#JqGrid', 585, 1080, '', '', function () { });
            $('#MinAllowRep').val(0);
            $('#LockMin').val(0);
            $('#OldPwdAllowCount').val(0);
            $('#SecurityExpiryTimeSec').val(0);
            $('#SecurityRetryCount').val(0);
            $('#Pwd').val(0);
            $('#PwdChangedAfterDays').val(0);
            $('#PwdStregth').val(0);
            $('#DisallowedChars').val(0);
            $('#EmpStatus').val(0);
            $('#MinLowCharsStrength').val(0);
            $('#MinUpCharsStrength').val(0);
            $('#Sessions').val(0);
            $('#OTPOnEmailAppl').val(0);
            $('#OTPRetainDays').val(0);
            $('#OTPActiveTime').val(0);
            $('#ApplicationUIList').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 595, "");
            event.preventDefault();

        });

        $('#Edit').on('click', function () {
            AssignData();
            $('#Create_Dialog').P2BEditModalDialog('@Url.Action("Edit")', dt.Id, '@Url.Action("EditSave")', "Password Policy Data", '#frmPasswordPolicy', dt.Id, 'Message : ', 'Request Falied :', '#JqGrid', 585, 1080, '', '', function (data) {
                //Compulsory for authorisation
                var val = null;
                $.each(data[0], function (i, k) {

                    if ((k.DualAuthenticationAppl) == false) {
                        $('.all_radio_button').addClass("disable");
                        $('#OTPRetainDays,#OldPwdAllowCount,#PwdStregth,#MinLowCharsStrength,#MinUpCharsStrength,#SecurityRetryCount,#SecurityExpiryTimeSec,#OTPOnEmailAppl,#PwdChangedAfterDays,#MinAllowRep,#LockMin,#Pwd,#DisallowedChars,#EmpStatus,#Sessions,#OTPActiveTime').attr('readonly', true);
                    }
                    else {
                        $('.all_radio_button').removeClass("disable");
                        $('#OTPRetainDays,#OldPwdAllowCount,#PwdStregth,#MinLowCharsStrength,#MinUpCharsStrength,#SecurityRetryCount,#SecurityExpiryTimeSec,#OTPOnEmailAppl,#PwdChangedAfterDays,#MinAllowRep,#LockMin,#Pwd,#DisallowedChars,#EmpStatus,#Sessions,#OTPActiveTime').attr('readonly', false);
                    }
                    if ((k.IsLoginSecurityAppl) == false) {
                        $('#MYdiv').fadeOut();
                    }
                    else {
                        $('#MYdiv').fadeIn();
                    }
                    if ((k.IsPasswordSecurityAppl) == false) {

                        $('#MYdiv1').fadeOut();
                    }
                    else {
                        $('#MYdiv1').fadeIn();
                    }

                    $('#frmPasswordPolicy').trigger("reset");
                    $('#MinPwdLength').val(k.MinPwdLength); // For Fetching TextBox value of form
                    $('#MinUpChars').val(k.MinUpChars);
                    $('#MinLowChars').val(k.MinLowChars);
                    $('#MinNoNos').val(k.MinNoNos);
                    $('#MinSymbols').val(k.MinSymbols);
                    $('#AllowAttempt').val(k.AllowAttempt);

                    $('#Authority').val(k.Authority);
                    $('#BadLogin').val(k.BadLogin);
                    $('#DisallowedChars').val(k.DisallowedChars);
                    $('#EmpStatus').val(k.EmpStatus);
                    $('#LockMin').val(k.LockMin);
                    $('#MinAllowRep').val(k.MinAllowRep);
                    $('#OldPwdAllowCount').val(k.OldPwdAllowCount);
                    $('#OTPActiveTime').val(k.OTPActiveTime);
                    //$('#OTPOnEmailAppl').val(k.OTPOnEmailAppl);
                    $('#OTPRetainDays').val(k.OTPRetainDays);
                    $('#MinLowCharsStrength').val(k.MinLowCharsStrength);
                    $('#MinUpCharsStrength').val(k.MinUpCharsStrength);
                    $('#PolicyName').val(k.PolicyName);
                    $('#Pwd').val(k.Pwd);
                    $('#PwdChangedAfterDays').val(k.PwdChangedAfterDays);
                    $('#PwdStregth').val(k.PwdStregth);
                    $('#SecurityRetryCount').val(k.SecurityRetryCount);
                    $('#Sessions').val(k.Sessions);
                    $('#SecurityExpiryTimeSec').val(k.SecurityExpiryTimeSec);


                    $('[name="MultipleLogin"]').val([k.MultipleLogin]).button().button("refresh"); // For Fetching RadioButtons value of form
                    $('[name="OldPwdAllow"]').val([k.OldPwdAllow]).button().button("refresh");
                    $('[name="SecurityQuestion"]').val([k.SecurityQuestion]).button().button("refresh");
                    $('[name="Captcha"]').val([k.Captcha]).button().button("refresh");
                    $('[name="AllowEncryption"]').val([k.AllowEncryption]).button().button("refresh");
                    $('[name="OTPOnEmailAppl"]').val([k.OTPOnEmailAppl]).button().button("refresh");
                    $('[name="CharsOnly"]').val([k.CharsOnly]).button().button("refresh");
                    $('[name="DefaultPasswordChangeInit"]').val([k.DefaultPasswordChangeInit]).button().button("refresh");
                    $('[name="DualAuthenticationAppl"]').val([k.DualAuthenticationAppl]).button().button("refresh");
                    $('[name="Forgot"]').val([k.Forgot]).button().button("refresh");
                    $('[name="IsLoginSecurityAppl"]').val([k.IsLoginSecurityAppl]).button().button("refresh");
                    $('[name="IsPasswordRotationSecurity"]').val([k.IsPasswordRotationSecurity]).button().button("refresh");
                    $('[name="IsPasswordSecurityAppl"]').val([k.IsPasswordSecurityAppl]).button().button("refresh");
                    $('[name="Lock"]').val([k.Lock]).button().button("refresh");
                    $('[name="LoginOTPAppl"]').val([k.LoginOTPAppl]).button().button("refresh");
                    $('[name="LoginRandomNoGenAppl"]').val([k.LoginRandomNoGenAppl]).button().button("refresh");
                    $('[name="LoginSecurityQuestionAppl"]').val([k.LoginSecurityQuestionAppl]).button().button("refresh");
                    $('[name="OTPOnMobileAppl"]').val([k.OTPOnMobileAppl]).button().button("refresh");
                    $('[name="OTPRetainDaysAppl"]').val([k.OTPRetainDaysAppl]).button().button("refresh");
                    $('[name="PasswordOTPAppl"]').val([k.PasswordOTPAppl]).button().button("refresh");
                    $('[name="PasswordRandomNoGenAppl"]').val([k.PasswordRandomNoGenAppl]).button().button("refresh");
                    $('[name="PasswordSecurityQuestion"]').val([k.PasswordSecurityQuestion]).button().button("refresh");
                    $('[name="StartNumericAppl"]').val([k.StartNumericAppl]).button().button("refresh");
                    $('[name="Suspend"]').val([k.Suspend]).button().button("refresh");
                    console.log(k.ApplicationUILK)
                    $('#ApplicationUIList').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 595, k.ApplicationUILK);

                });

                $('#Autho_Allow').val(data[3]);
            });

        });

        $('#View').button().click(function (event) {
            AssignData();
            $('#Create_Dialog').P2BViewModalDialog('@Url.Action("Edit")', dt.Id, 'View Password Policy Data', '', '', 585, 1080, '#Autho', '@Url.Action("AuthSave")', dt.Id, '', '', '', '', '#JqGrid', function (data) {
                $.each(data[0], function (i, k) {


                    if ((k.IsLoginSecurityAppl) == false) {
                        $('#MYdiv').fadeOut();
                    }
                    else {
                        $('#MYdiv').fadeIn();
                    }
                    if ((k.IsPasswordSecurityAppl) == false) {

                        $('#MYdiv1').fadeOut();
                    }
                    else {
                        $('#MYdiv1').fadeIn();
                    }

                    $('#frmPasswordPolicy').trigger("reset");
                    $('#MinPwdLength').val(k.MinPwdLength); // For Fetching TextBox value of form
                    $('#MinUpChars').val(k.MinUpChars);
                    $('#MinLowChars').val(k.MinLowChars);
                    $('#MinNoNos').val(k.MinNoNos);
                    $('#MinSymbols').val(k.MinSymbols);
                    $('#AllowAttempt').val(k.AllowAttempt);
                    $('#Authority').val(k.Authority);
                    $('#BadLogin').val(k.BadLogin);
                    $('#DisallowedChars').val(k.DisallowedChars);
                    $('#EmpStatus').val(k.EmpStatus);
                    $('#LockMin').val(k.LockMin);
                    $('#MinAllowRep').val(k.MinAllowRep);
                    $('#OldPwdAllowCount').val(k.OldPwdAllowCount);
                    $('#OTPActiveTime').val(k.OTPActiveTime);
                    //$('#OTPOnEmailAppl').val(k.OTPOnEmailAppl);
                    $('#OTPRetainDays').val(k.OTPRetainDays);
                    $('#MinLowCharsStrength').val(k.MinLowCharsStrength);
                    $('#MinUpCharsStrength').val(k.MinUpCharsStrength);
                    $('#PolicyName').val(k.PolicyName);
                    $('#Pwd').val(k.Pwd);
                    $('#PwdChangedAfterDays').val(k.PwdChangedAfterDays);
                    $('#PwdStregth').val(k.PwdStregth);
                    $('#SecurityRetryCount').val(k.SecurityRetryCount);
                    $('#Sessions').val(k.Sessions);
                    $('#SecurityExpiryTimeSec').val(k.SecurityExpiryTimeSec);

                    $('[name="MultipleLogin"]').val([k.MultipleLogin]).button().button("refresh"); // For Fetching RadioButtons value of form
                    $('[name="OldPwdAllow"]').val([k.OldPwdAllow]).button().button("refresh");
                    $('[name="SecurityQuestion"]').val([k.SecurityQuestion]).button().button("refresh");
                    $('[name="Captcha"]').val([k.Captcha]).button().button("refresh");
                    $('[name="AllowEncryption"]').val([k.AllowEncryption]).button().button("refresh");
                    $('[name="OTPOnEmailAppl"]').val([k.OTPOnEmailAppl]).button().button("refresh");
                    $('[name="CharsOnly"]').val([k.CharsOnly]).button().button("refresh");
                    $('[name="DefaultPasswordChangeInit"]').val([k.DefaultPasswordChangeInit]).button().button("refresh");
                    $('[name="DualAuthenticationAppl"]').val([k.DualAuthenticationAppl]).button().button("refresh");
                    $('[name="Forgot"]').val([k.Forgot]).button().button("refresh");
                    $('[name="IsLoginSecurityAppl"]').val([k.IsLoginSecurityAppl]).button().button("refresh");
                    $('[name="IsPasswordRotationSecurity"]').val([k.IsPasswordRotationSecurity]).button().button("refresh");
                    $('[name="IsPasswordSecurityAppl"]').val([k.IsPasswordSecurityAppl]).button().button("refresh");
                    $('[name="Lock"]').val([k.Lock]).button().button("refresh");
                    $('[name="LoginOTPAppl"]').val([k.LoginOTPAppl]).button().button("refresh");
                    $('[name="LoginRandomNoGenAppl"]').val([k.LoginRandomNoGenAppl]).button().button("refresh");
                    $('[name="LoginSecurityQuestionAppl"]').val([k.LoginSecurityQuestionAppl]).button().button("refresh");
                    $('[name="OTPOnMobileAppl"]').val([k.OTPOnMobileAppl]).button().button("refresh");
                    $('[name="OTPRetainDaysAppl"]').val([k.OTPRetainDaysAppl]).button().button("refresh");
                    $('[name="PasswordOTPAppl"]').val([k.PasswordOTPAppl]).button().button("refresh");
                    $('[name="PasswordRandomNoGenAppl"]').val([k.PasswordRandomNoGenAppl]).button().button("refresh");
                    $('[name="PasswordSecurityQuestion"]').val([k.PasswordSecurityQuestion]).button().button("refresh");
                    $('[name="StartNumericAppl"]').val([k.StartNumericAppl]).button().button("refresh");
                    $('[name="Suspend"]').val([k.Suspend]).button().button("refresh");
                    console.log(k.ApplicationUILK)
                    $('#ApplicationUIList').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 595, k.ApplicationUILK);
                });

                $('#Autho_Allow').val(data[3]);


            });
        });

        $('input[name="IsLoginSecurityAppl"]:radio').change(function () {
            if (this.value == "true") {
                $('#MYdiv').fadeIn();
            }
            else {

                $('#MYdiv').fadeOut();
            }
        });

        $('input[name="IsPasswordSecurityAppl"]:radio').change(function () {

            if (this.value == "true") {
                $('#MYdiv1').fadeIn();
            }
            else {

                $('#MYdiv1').fadeOut();
            }
        });

        $('input[name="DualAuthenticationAppl"]:radio').change(function () {
            if (this.value == "true") {
                $('.all_radio_button').removeClass("disable");
                $('#OTPRetainDays,#OldPwdAllowCount,#PwdStregth,#MinLowCharsStrength,#MinUpCharsStrength,#SecurityRetryCount,#SecurityExpiryTimeSec,#OTPOnEmailAppl,#PwdChangedAfterDays,#MinAllowRep,#LockMin,#Pwd,#DisallowedChars,#EmpStatus,#Sessions,#OTPActiveTime').attr('readonly', false);
            }
            else {
                $('#OTPRetainDays,#OldPwdAllowCount,#PwdStregth,#MinLowCharsStrength,#MinUpCharsStrength,#SecurityRetryCount,#SecurityExpiryTimeSec,#OTPOnEmailAppl,#PwdChangedAfterDays,#MinAllowRep,#LockMin,#Pwd,#DisallowedChars,#EmpStatus,#Sessions,#OTPActiveTime').attr('readonly', true);
                $('.all_radio_button').addClass("disable");


            }
        });

        function AssignData() {
            dt = $('#JqGrid').P2BGrid.P2BEdDelV('#JqGrid', 'Id');
        };

        $('#Delete').button().click(function (event) {
            dt = $('#JqGrid').P2BGrid.P2BEdDelV('#JqGrid', 'ID');
            $('#frmJVParameter').P2BDeleteModalDialog('@Url.Action("Delete")', dt.Id, 'Are you sure want to delete record???', 'Message : ', 'Request failed', '#JqGrid');
        });

        $('.popup_radio_button').controlgroup();
        $('.drop').selectmenu().selectmenu('menuWidget');

    });
</script>
