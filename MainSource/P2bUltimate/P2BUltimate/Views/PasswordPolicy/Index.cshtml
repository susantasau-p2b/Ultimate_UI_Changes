        <form id="frmPasswordPolicy" method="post" name="frmPasswordPolicy">
            <div class="pwdmrgn">
                <div class="section_Method">
                    <div class="content_margine row3">
                        <div class="row3 content_margine">@Html.LabelFor(m => m.PolicyName, "PolicyName :", new { @class = "popup-content-label must" })@Html.TextBoxFor(m => m.PolicyName, new { @class = "popup-content-txtbox must" })</div>
                    </div>
