            $('#DateOfAppl').attr('readonly', 'readonly');
            $('#DateOfAppl').TodayDate();
            $('#LTCTypelist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "Lookup")', 3000, "");




            event.preventDefault();

        });
