</html>
<script>
    $(document).ready(function (e) {
        $('button').button();
        $('#MyTable').MultiLevelInlineEditGrid({
            url: "@Url.Action("Emp_Grid")",
            editurl: "@Url.Action("EditGridDetails", "LTCSettlementClaim")",
            htmurl: "@Url.Action("partial", "LTCSettlementClaim")",
            submiturl: "@Url.Action("GridEditSave", "LTCSettlementClaim")",
            columnname: ["Code", "Name", "Location", "Id"],
            childheader: ["ReqDate", "LTCType", "EligibleAmt", "AdvanceAmt", "Remark", "LTCEncashAmount", "LTC_Sanction_Amt", "LTC_Settle_Amt", "LTCAdvAmt", "Id"],
            //childheader: ["Id", "LoanAdvHead", "RequisitionDate", "EnforcementDate", "LoanAppliedAmt", "LoanSanctionedAmt", "MonthlyInstallmentAmt", "TotalInstallment", "LoanAccBranch", "LoanAccNo"],
            childurl: "@Url.Action("Get_LTCSettlementClaim", "LTCSettlementClaim")",
            tableheader: "LTC Settlement Claim",
            childurlColumnNo: "4"
        });

        $.DataTableChildManipulation = function (init, id, parentid, htmurl, editurl, submiturl) {
            var parent_element = $(init).parents('td:last').parent().prev();
            var chhassa = $(parent_element).find('td:eq(4)').html();
            console.log(parent_element);
            var dt = $('#MyTable').DataTable();
            var htmlcomponant = {
                init: init,
                Id: id,
                idx: parentid,
                htmurl: htmurl,
                editurl: editurl,
                submiturl: submiturl,
            };
            if ($(init).hasClass('ui-icon-newwin')) {
                $('<div></div>').PartialCreateDialog({
                    height: 'auto',
                    width: 'auto',
                    form: "#Edit-LTCAdvanceClaim",
                    htmlurl: "@Url.Action("GridPartial", "LTCAdvanceClaim")",
                    state: "open",
                    mode: "View",
                    editurl: "@Url.Action("GridEditData", "LTCAdvanceClaim")",
                    submiturl: "@Url.Action("GridEditSave", "LTCAdvanceClaim")",
                    editdata: id,
                    forwarddata: id,
                    returndatafunction: function (returndata) {
                        $.each(returndata.returndata, function (i, k) {
                            $('#Edit-ProposedPlace').val(k.ProposedPlace);
                            $('#Edit-LTCEligibleAmount').val(k.EligibleAmount);
                            $('#Edit-LTCAdvanceAmount').val(k.AdvanceAmount);
                            $('#Edit-Remark').val(k.Remark);


                        });
                    },
                });
            } else if ($(init).hasClass('ui-icon-pencil')) {
                // //debugger
                $('<div></div>').PartialCreateDialog({
                    height: 640,
                    width: 570,
                    form: "#frmLTCSettlmentClaimPartial",
                    htmlurl: "@Url.Action("GridPartial", "LTCSettlementClaim")",
                    state: "open",
                    mode: "Edit",
                    editurl: "@Url.Action("GridEditData", "LTCSettlementClaim")",
                    submiturl: "@Url.Action("GridEditSave", "LTCSettlementClaim")",
                    editdata: id,
                    forwarddata: id,
                    returndatafunction: function (data) {
                        $.each(data, function (i, k) {
                            console.log("employeeids");
                            console.log(chhassa);
                            $('#EmployeeSettleId').val(chhassa);
                            $('#LTCTypelistPartial').P2BSelectMenuAppend("@Url.Action("GetLookupValue", "lookup")", 3000, k.LTCType); // Dropdown bind
                            $.get("@Url.Action("GetEmpLTCBlock")", { data: chhassa }, function (data) {
                                $('#txtLTCBlockPartial').val(data.Id);
                                $('#BlockIdPartial').val(data.Id);
                                $('#LvReqListPartial').P2BSelectMenuAppend('@Url.Action("GetLVReqLKDetails", "LTCAdvanceClaim")', chhassa, data.Id); // To bind dropdown on page load
                                $('#txtLTCBlockPartial').val(data.FullDetails).attr("readonly", "readonly");
                            });
                            $('#ltcclaimamtaprtial').val(k.ltcclaimamt);
                            $('#ltceligibleamtpartial').val(k.EligibleAmt);
                            if (k.ReqDate != null) { $('#DateApplPartial').val(DateConvert(k.ReqDate)); }
                            $('#ltcnoofdayspartial').val(k.Noofdays)
                            $('#ltcEncashamtprtial').val(k.LTCEncashAmount);
                            $('#ltcadvamtpartial').val(k.LTCAdvAmt);
                            $('#ltcsettleamtpartial').val(k.LTC_Settle_Amt);
                            $('#ltcremarkpartial').val(k.Remark);
                            $('#LTCAdvanceClaimListPartial').P2BLookUpEncapsulate('#LTCAdvanceClaimListPartial', 'LTCAdvanceClaimListPartial', k.LTCAdvanceClaimid, k.LTCAdvanceClaimfulldetails, 'LTCAdvanceClaimListPartial', '#Edit-Rent-HRATransTPartial,#Remove-Rent-HRATransTPartial', 'A');
                            $('#JourneyDetailsListPartial').P2BLookUpEncapsulate('#JourneyDetailsListPartial', 'JourneyDetailsListPartial', k.JourneydetailsId, k.JourneydetailsFulldetails, 'JourneyDetailsListPartial', '#Edit-Rent-HRATransTPartial,#Remove-Rent-HRATransTPartial', 'A');
                            $.each(k.travelhoteldetails, function (l, m) {
                                $('#HotelBookingDetailsListPartial').P2BLookUpEncapsulate('#HotelBookingDetailsListPartial', 'HotelBookingDetailsListPartial', m.id, m.fulldetails, 'HotelBookingDetailsListPartial', '#Edit-Rent-HRATransTPartial,#Remove-Rent-HRATransTPartial', 'N');

                            });
                            @*$.each(k.Employeedocument, function (l, m) {
                                $('#CandidateDocumentslistpartial').P2BLookUpEncapsulate('#CandidateDocumentslistpartial', 'CandidateDocumentslistpartial', m.id, m.Fulldetails, 'CandidateDocumentslistpartial', '#Edit-Rent-HRATransTPartial,#Remove-Rent-HRATransTPartial', 'N');

                            })
                            $('#CountryListpartial').P2BSelectMenuAppend("@Url.Action("PopulateDropDownList")", "", k.Country, '');
                            $('#StateListpartial').P2BSelectMenuAppend("@Url.Action("PopulateDropDownListState")", "", k.State, '');
                            $('#CityListpartial').P2BSelectMenuAppend("@Url.Action("PopulateDropDownListCity")", "", k.City, '');*@

                        });
                    },
                    submitfun: function (data) {
                        var row = dt.row(parent_element);
                        if (row.child.isShown()) {
                            parent_element.removeClass('details');
                            row.child.hide();
                        }
                    }
                });
            } else if ($(init).hasClass('ui-icon-trash')) {
                // alert(chhassa);
                $('<div></div>').PartialCreateDialog({
                    state: "open",
                    mode: "Delete",
                    submiturl: "@Url.Action("Delete")",
                    forwarddata: id,
                    editdata: chhassa,
                    submitfun: function (data) {
                        var row = dt.row(parent_element);
                        if (row.child.isShown()) {
                            parent_element.removeClass('details');
                            row.child.hide();
                        }
                    }
                });
            }
        };

        function AssignData() {
            dt = $('#JqGrid').P2BGrid.P2BEdDelV('#JqGrid', 'Id');
        }

        $('#Advance-Filter-Symbol').on('click', function () {
            $("<div></div>").FilterDialog({
                width: 1000,
                height: 400,
                title: "Filter",
                htmlurl: "@Url.Action("Advance_partial", "Transcation")",
                returnat: function (data) {
                    //console.log(data);
                    var forwarddata = JSON.stringify(data);
                    $('#Employee-Table').P2BTransactionTableDynamic('#Employee-Search', '#Case-Employee', './Transcation/Get_Employelist_h?geo_id=' + forwarddata + '', "");
                },
                renderat: "#FormFilter", hierarchy: true,
            });
        });

        $('#Employee-Table').on('click', 'input:checkbox:gt(0)', function (e) {
            $('input[type="checkbox"]').not(this).prop("checked", false);
            if (this.checked) {
                //alert($(this).val());
                var id = $(this).val();
                $.get("@Url.Action("GetEmpLTCBlock")", { data: id }, function (data) {
                    $('#txtLTCBlock').val(data.Id);
                    $('#BlockId').val(data.Id);
                    $('#LvReqList').P2BSelectMenuAppend('@Url.Action("GetLVReqLKDetails", "LTCAdvanceClaim")', id, data.Id); // To bind dropdown on page load
                    $('#txtLTCBlock').val(data.FullDetails).attr("readonly", "readonly");
                });
            }
        });
        function DateConvert(JsonDate) {
            var date = new Date(parseInt(JsonDate.substr(6)));
            var displayDate = $.datepicker.formatDate("dd/mm/yy", date);
            return displayDate;
        };

        $('#Edit-HotelBookingDetails-LTCSettlementClaim').button().on("click", function (event) {
            var a = [], b;
            a = $('#HotelBookingDetailsList').P2BGetTableDataonSelectedRow('#HotelBookingDetailsList', 0);
            b = a[0];

            $('<div></div>').P2BPatialEditModalDialog('@Url.Action("partial_TravelHotelBooking", "LTCSettlementClaim")', '@Url.Action("GethotelbookingData", "TravelHotelBooking")', b, '@Url.Action("EditSave", "TravelHotelBooking")', 'Edit HotelBooking  Details', '#frmTravelHotelBookingDetails', b, 'Message :', 'Request Falied :', '#JqGrid', 400, 500, '#frmLTCSettlmentClaim', '#HotelBookingDetailsList', 'HotelBookingDetailsList', 'HotelBookingDetailsList', 'N', '#Edit-JourneyObject-JourneyDetails,#Remove-JourneyObject1-JourneyDetails', function (data) {
