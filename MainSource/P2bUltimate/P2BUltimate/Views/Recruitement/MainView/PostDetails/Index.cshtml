            </div>
            @* <div class="row6 content_margine"> @Html.LabelFor(m => m.FuncStruct.JobPosition, "Job Position :", new { @class = "popup-content-label   " })@Html.TextBoxFor(m => m.FuncStruct.JobPosition, new { @class = "popup-content-textbox   " })  </div>*@
            <div class="row6 content_margine"> @Html.LabelFor(m => m.RequestVacancies, "Vacancies :", new { @class = "popup-content-label   must" })@Html.TextBoxFor(m => m.RequestVacancies, new { @class = "popup-content-textbox   must " })  </div>

            @*<div class="row2 content_margine">
                     <label>Experiance</label><br />
                 </div>

                     <div class="row6 content_margine">@Html.LabelFor(m => m.ExpFilter, "Experiance Criteria :", new { @class = "popup-content-label" })<select id="ExpFilterlist" name="ExpFilterlist" class=" " style="width:161px;"><option></option></select>    </div>
                    <br />
                      <div class="row6 content_margine"> @Html.LabelFor(m => m.ExpYearFrom, "From :", new { @class = "popup-content-label  " })@Html.TextBoxFor(m => m.ExpYearFrom, new { @class = "popup-content-textbox    " })        </div>
                     <br />
                       <div class="row6 content_margine"> @Html.LabelFor(m => m.ExpYearTo, "To :", new { @class = "popup-content-label    " })@Html.TextBoxFor(m => m.ExpYearTo, new { @class = "popup-content-textbox    " })          </div>
                <br />*@<br />
