            <div class="row3 content_margine">
                <div id="QualificationlistM_old"></div>
                @Html.LabelFor(m => m.Qualification, "Qualification :", new { @class = "popup-content-label-2" })
                <div class="popup-content-table">
                    <table class="lookuptableselected" id="QualificationlistM">
                        <tr><th>Id</th><th>LookUp Selected Description</th></tr>
                    </table>
                </div>
                <div class="icon-row">
                    <button data-p2btooltip="Lookup " class="  popup-content-icon-lookup" id="Lookup-Qualification-PostDetails"><span class="ui-icon ui-icon-folder-open popup-content-iconmark"></span></button>
