            <div class="row6 content_margine">   @Html.LabelFor(m => m.Gender, "Gender :", new { @class = "popup-content-label " })<select id="Gender_drop" name="Gender_drop" @*class="must"*@ style="width:161px;"><option></option></select>    </div>
            <br />
            <div class="row6 content_margine">   @Html.LabelFor(m => m.MaritalStatus, "Maratial Status :", new { @class = "popup-content-label " })<select id="MaritalStatus_drop" name="MaritalStatus_drop" @*class="must"*@ style="width:161px;"><option></option></select>    </div>

            <br />
            <div class="row3 content_margine">
                <div id="CategoryPostlist_old"></div>@Html.LabelFor(m => m.CategoryPost, " CategoryPost :", new { @class = "popup-content-label-2" })
                <div class="popup-content-table">
