</html>

<script>
    $(document).ready(function () {

        //$('#Code').ValidateTextBox({ maxlength: 10, alphanum: true });
        //$('#Code').MaxLength(10);
        //// $('#Name').ForceOnlyAlpha();
        //// $('#Name').MaxLength(50);
        $('#NoOfVacancies').ForceOnlyNumeric().MaxLength(3);
        $('#ExpYearFrom').ForceOnlyNumeric().MaxLength(3);
        $('#ExpYearTo').ForceOnlyNumeric().MaxLength(3);
        $('#AgeFrom').ForceOnlyNumeric().MaxLength(3);
        $('#AgeTo').ForceOnlyNumeric().MaxLength(3);
        $('#Narration').MaxLength(50);
        $('#PostRequestDate').P2BDatePicker();

        //$('#Name').ValidateTextBox({ maxlength: 50, name: true });
    });
    $(document).ready(function (e) {
        $('button').button();
        var ColNames = [];
        var ColModel = [];
        var SortName, Caption, url, width, height;
        var btndisablenames = "#Remove-JobPosition-PostDetails,#Edit-CategoryPost-PostDetails,#Remove-CategoryPost-PostDetails,#Edit-CategorySplPost-PostDetails,#Remove-CategorySplPost-PostDetails,#Edit-Skill-PostDetails,#Remove-Skill-PostDetails,#Edit-Qualification-PostDetails,#Remove-Qualification-PostDetails";
        var namelookuptable = '#JobPositionMlist,#QualificationlistM,#SkillList,#CategoryPostlist,#CategorySplPostlist';
        ColModel = ['PostCode', 'Jobdescription', 'NoOfVacancies', 'Id'];
        ColNames = ['PostCode', 'Job description', 'NoOfVacancies', 'Id'];
        SortName = 'Id';
        Caption = 'Manpower Post Creation';
        url = '@Url.Action("P2BGrid")';
        width = 1100;
        height = 150;
        $('#JqGrid').P2BGrid(ColNames, ColModel, SortName, Caption, url, width, height);
        $('#CreationDate').P2BDatePicker();
        function AssignData() {
            dt = $('#JqGrid').P2BGrid.P2BEdDelV('#JqGrid', 'Id');
        }
        function DateConvert(JsonDate) {
            var date = new Date(parseInt(JsonDate.substr(6)));
            var displayDate = $.datepicker.formatDate("dd/mm/yy", date);
            return displayDate;
        };
        $('#ExpYearTo').attr('disabled', 'disabled');
        $('#AgeTo').attr('disabled', 'disabled');
        $("#ExpFilterlist").on('selectmenuchange', function () {
            if ($('option:selected', this).text() == "BETWEEN") {
                $('#ExpYearTo').removeAttr('disabled');
            }
            if ($('option:selected', this).text() == "GREATER THAN" || $('option:selected', this).text() == "LESS EQUAL" ||
                 $('option:selected', this).text() == "LESS THAN" || $('option:selected', this).text() == "EQUAL TO" || $('option:selected', this).text() == "GREATER EQUAL") {
                $('#ExpYearTo').attr('disabled', 'disabled');
                $('#ExpYearTo').removeClass('must');
                $('#ExpYearTo').text("");
            }

        });

        $("#RangeFilterlist").on('selectmenuchange', function () {
            if ($('option:selected', this).text() == "BETWEEN") {
                $('#AgeTo').removeAttr('disabled');
            }
            if ($('option:selected', this).text() == "GREATER THAN" || $('option:selected', this).text() == "LESS EQUAL" ||
                 $('option:selected', this).text() == "LESS THAN" || $('option:selected', this).text() == "EQUAL TO" || $('option:selected', this).text() == "GREATER EQUAL") {
                $('#AgeTo').attr('disabled', 'disabled');
                $('#AgeTo').removeClass('must');
                $('#AgeTo').val("");
            }

        });

        $('#Load').on('click', function (e) {
            // alert($('#Financial_id').val());

            if (!$("#Financial_id").val()) {
                alert("Please Select Recruitment Year.");
                return false;
            }
            $('#JqGrid').PostGridData({ url: url, filter: $('#txtPayMonth').val() });
        });

        $('#Create_Dialog').P2BCreateDialog();
        $('#Create').on("click", function (event) {
            if (!$("#Financial_id").val()) {
                alert("Please Select Recruitment Year.");
                return false;
            }
            $('#frmPostDetails').trigger("reset");
            $('#Create_Dialog').P2BCreateDialog('', '', '', '', 'Create Manpower Post Creation', 'open', '@Url.Action("Create")', '#frmPostDetails', 'Message :', 'Request Falied :', '#JqGrid', 550, 810, namelookuptable, btndisablenames, function (data) { });
            $('#ExpFilterlist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 1061, ""); // To bind dropdown on page load
            $('#RangeFilterlist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 1058, "");
            $('#MaritalStatus_drop').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 128, "");
            $('#Gender_drop').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 129, "");
            $('#PostSourceTypelist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 129, "");

            event.preventDefault();
        });

        // Main page edit
        $('#Edit').on('click', function () {
            AssignData();
            $('#Create_Dialog').P2BEditModalDialog('@Url.Action("Edit")', dt.Id, '@Url.Action("EditSave")', "Edit Manpower Post Creation Data", '#frmPostDetails', dt.Id, 'Message : ', 'Request Falied :', '#JqGrid', 500, 810, '', btndisablenames, function (data) {
                // setTimeout(function () {
                $.each(data[0], function (i, k) {

                    $('#RequestVacancies').val(k.Vaccancy);
                    $('#NoOfVacancies').attr('readonly', 'readonly');
                    $('#ExpYearFrom').val(k.ExpYearFrom);
                    $('#ExpYearTo').val(k.ExpYearTo);
                    $('#AgeFrom').val(k.AgeFrom);
                    $('#AgeTo').val(k.AgeTo);
                    $('#Narration').val(k.Narration);
                    $('#PostCode').val(k.PostCode);
                    if (k.CreationDate != null) {
                        $('#CreationDate').val(DateConvert(k.CreationDate));
                    }

                    @*$('#RangeFilterlist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 1058, k.AgeCriteria);
                    $('#MaritalStatus_drop').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 10588, k.MaritalStatus_Id);
                    $('#Gender_drop').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 10589, k.Gender_Id);
                    $('#ExpFilterlist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 1061, k.ExpCriteria_Id);*@

                    $('#RangeFilterlist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 1058, k.RangeFilter_Id);
                    $('#MaritalStatus_drop').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 128, k.MaritalStatus_Id);
                    $('#Gender_drop').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 129, k.Gender_Id);
                    $('#ExpFilterlist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 1061, k.ExpFilter_Id);
                    $('#PostSourceTypelist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 129, "");
                });

                //partial page bind
                $.each(data[1], function (i, k) {

                    $('#JobPositionMlist').P2BLookUpEncapsulate('#JobPositionMlist', 'JobPositionMlist', k.JobPos_Id, k.JobPos_FullAddress, 'JobPositionMlist', '#Edit-JobPosition-PostDetails,#Remove-JobPosition-PostDetails', 'N');
                    //});
                    //$.each(data[2], function (i, k) {

                    $('#QualificationlistM').P2BLookUpEncapsulate('#QualificationlistM', 'QualificationlistM', k.QualificationDetails_Id, k.QualificationDetails_val, 'QualificationlistM', '#Edit-Qualification-PostDetails,#Remove-Qualification-PostDetails', 'A');
                    $('#SkillList').P2BLookUpEncapsulate('#SkillList', 'SkillList', k.SkillDetails_Id, k.SkillDetails_val, 'SkillList', '#Edit-Skill-PostDetails,#Remove-Skill-PostDetails', 'A');
                    $('#CategoryPostlist').P2BLookUpEncapsulate('#CategoryPostlist', 'CategoryPostlist', k.CategoryDetails_Id, k.CategoryDetails_val, 'CategoryPostlist', '#Edit-CategoryPost-PostDetails,#Remove-CategoryPost-PostDetails', 'A');
                    $('#CategorySplPostlist').P2BLookUpEncapsulate('#CategorySplPostlist', 'CategorySplPostlist', k.CategorySplDetails_Id, k.CategorySplDetails_val, 'CategorySplPostlist', '#Edit-CategorySpl-PostDetails,#Remove-CategorySpl-PostDetails', 'A');

                });
                //alert(data[3]);
                $('#Autho_Allow').val(data[2]); //Compulsory for authorisation
                // }, 2000);
            });
        });


        //View main page
        $('#View').button().click(function (event) {
            AssignData();
            $('#Create_Dialog').P2BViewModalDialog('@Url.Action("Edit")', dt.Id, 'View Manpower Post Creation Data', '#JobPositionMlist,#QualificationlistM,#SkillList,#CategoryPostlist,#CategorySplPostlist', '', 500, 810, '#Autho', '@Url.Action("AuthSave")', dt.Id, '', '', '', '', '#JqGrid', function (data) {

                $.each(data[0], function (i, k) {

                    $('#RequestVacancies').val(k.Vaccancy);
                    $('#NoOfVacancies').attr('readonly', 'readonly');
                    $('#ExpYearFrom').val(k.ExpYearFrom);
                    $('#ExpYearTo').val(k.ExpYearTo);
                    $('#AgeFrom').val(k.AgeFrom);
                    $('#AgeTo').val(k.AgeTo);
                    $('#Narration').val(k.Narration);
                    $('#PostCode').val(k.PostCode);
                    if (k.CreationDate != null) {
                        $('#CreationDate').val(DateConvert(k.CreationDate));
                    }

                    $('#RangeFilterlist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 1058, k.RangeFilter_Id);
                    $('#MaritalStatus_drop').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 128, k.MaritalStatus_Id);
                    $('#Gender_drop').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 129, k.Gender_Id);
                    $('#ExpFilterlist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 1061, k.ExpFilter_Id);
                    $('#PostSourceTypelist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 129, "");
                });

                //partial page bind
                $.each(data[1], function (i, k) {
                    $('#JobPositionMlist').P2BLookUpEncapsulate('#JobPositionMlist', 'JobPositionMlist', k.JobPos_Id, k.JobPos_FullAddress, 'JobPositionMlist', 'view', 'N');
                    //});
                    //$.each(data[2], function (i, k) {
                    $('#QualificationlistM').P2BLookUpEncapsulate('#QualificationlistM', 'QualificationlistM', k.QualificationDetails_Id, k.QualificationDetails_val, 'QualificationlistM', 'view', 'N');
                    $('#SkillList').P2BLookUpEncapsulate('#SkillList', 'SkillList', k.SkillDetails_Id, k.SkillDetails_val, 'SkillList', 'view', 'A');
                    $('#CategoryPostlist').P2BLookUpEncapsulate('#CategoryPostlist', 'CategoryPostlist', k.CategoryDetails_Id, k.CategoryDetails_val, 'CategoryPostlist', 'view', 'A');
                    $('#CategorySplPostlist').P2BLookUpEncapsulate('#CategorySplPostlist', 'CategorySplPostlist', k.CategorySplDetails_Id, k.CategorySplDetails_val, 'CategorySplPostlist', 'view', 'A');

                });
                //alert(data[3]);
                $('#Autho_Allow').val(data[2]); //Compulsory for authorisation

            });
        });

        //delete main page
        $('#Delete').button().click(function (event) {
            dt = $('#JqGrid').P2BGrid.P2BEdDelV('#JqGrid', 'Id');
            $('#Create_Dialog').P2BDeleteModalDialog('@Url.Action("Delete")', dt.Id, 'Are you sure want to remove record???', 'Message : ', 'Request failed', '#JqGrid', 200, 200);
            event.preventDefault();
        });


        // --------------- Qualification lookup start ----------

