            <input id="Autho_Allow" type="hidden" name="Autho_Allow" />

            @*<div class="row6 content_margine">    @Html.LabelFor(m => m.PostSourceType, "Source Type :", new { @class = "popup-content-label " })<select id="PostSourceTypelist" name="PostSourceTypelist" class=" " style="width:161px;"><option></option></select>     </div>*@
