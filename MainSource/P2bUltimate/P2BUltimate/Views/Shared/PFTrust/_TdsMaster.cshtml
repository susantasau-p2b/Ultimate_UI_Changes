@{
    Layout = null;
}


@model P2B.PFTRUST.PFTTDSMaster


<html>
<head>
    <style>


        .contact_popup-content-textbox {
            margin-bottom: 20px;
            min-width: 16%;
        }


        .contact_popup-content-label {
            display: block;
            margin: 1px;
            float: left;
            width: 142px;
        }

        .contact_row {
            width: 397px;
        }

        .con_icon-row {
            position: absolute;
            left: 64px;
            top: 83px;
        }

        .date_label {
            margin-left: 40px;
            width: 95px;
        }

        .date_label1 {
            margin-left: 3px;
            width: 95px;
        }

        .date_textbox {
            margin-bottom: 20px;
            width: 80px;
            margin-left: 24px;
        }

        .heading {
            margin-bottom: 20px;
        }
    </style>


</head>


<body>
    <div>

        @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frmTdsMaster", name = "frmTdsMaster" }))
        {

            <div class="contact_row content_margine">
                @Html.LabelFor(m => m.TDSRate, "TDSRate : ", new { @class = "contact_popup-content-label" })
                @Html.TextBoxFor(m => m.TDSRate, new { @class = "contact_popup-content-textbox must", @maxlength = "100" })
            </div>
            <div class="contact_row content_margine">
                @Html.LabelFor(m => m.TaxableAccountCelling, "TaxableAccountCelling : ", new { @class = "contact_popup-content-label" })
                @Html.TextBoxFor(m => m.TaxableAccountCelling, new { @class = "contact_popup-content-textbox must", @maxlength = "100" })
            </div>
             <div class=" content_margine">
                @Html.LabelFor(m => m.IsIntOnIntTDSAppl, "IntOnIntTDSAppl :", new { @class = "popup-content-label must" })
                <div class="popup_radio_button radio_button_div_1">
                    <span class="popup_radio_button radio_button_div_1">
                        <input type="radio" id="radio1" name="IntOnIntTDSAppl" value="true" /><label for="radio1">Yes</label>
                        <input type="radio" id="radio2" name="IntOnIntTDSAppl" value="false" checked="checked" /><label for="radio2">No</label>
                    </span>
                </div>
            </div>
            
             <div class=" content_margine">
                @Html.LabelFor(m => m.IsOwnTDSAppl, "OwnTDSAppl :", new { @class = "popup-content-label must" })
                <div class="popup_radio_button radio_button_div_1">
                    <span class="popup_radio_button radio_button_div_1">
                        <input type="radio" id="radio11" name="OwnTDSAppl" value="true" /><label for="radio11">Yes</label>
                        <input type="radio" id="radio12" name="OwnTDSAppl" value="false" checked="checked" /><label for="radio12">No</label>
                    </span>
                </div>
            </div>
            
            <div class=" content_margine">
                @Html.LabelFor(m => m.IsOwnerTDSAppl, "OwnerTDSAppl :", new { @class = "popup-content-label must" })
                <div class="popup_radio_button radio_button_div_1">
                    <span class="popup_radio_button radio_button_div_1">
                        <input type="radio" id="radio9" name="OwnerTDSAppl" value="true" /><label for="radio9">Yes</label>
                        <input type="radio" id="radio10" name="OwnerTDSAppl" value="false" checked="checked" /><label for="radio10">No</label>
                    </span>
                </div>
            </div>
            
              <div class=" content_margine">
                @Html.LabelFor(m => m.IsVPFTDSAppl, "VPFTDSAppl :", new { @class = "popup-content-label must" })
                <div class="popup_radio_button radio_button_div_1">
                    <span class="popup_radio_button radio_button_div_1">
                        <input type="radio" id="radio7" name="VPFTDSAppl" value="true" /><label for="radio7">Yes</label>
                        <input type="radio" id="radio8" name="VPFTDSAppl" value="false" checked="checked" /><label for="radio8">No</label>
                    </span>
                </div>
            </div>
            
           
            
          



           


          



        }

    </div>


</body>



</html>

<script>









    $(document).ready(function () {

        $("#EffectiveFrom").P2BDatePicker();
        $("#EffectiveTo").P2BDatePicker();
        $('#TDSRate').val(0);
        $('#TDSRate').ForceOnlyNumeric();
        $('.popup_radio_button').controlgroup();
    });

</script>