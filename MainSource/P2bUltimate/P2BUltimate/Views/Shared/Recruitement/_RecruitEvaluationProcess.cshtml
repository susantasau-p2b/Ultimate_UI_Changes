                // setTimeout(function () {
                //to bind data according to selected id
                $.each(data[0], function (i, k) {
                    $('#FormShortlistingCriteria').trigger("reset");
                    $('#AgeFrom').val(k.txtAgeFrom);
                    $('#AgeTo').val(k.txtAgeTo);
                    $('#username').val(k.txtNarration);
                    $('#ExpYearFrom').val(k.txtExpYearFrom);
                    $('#ExpYearTo').val(k.txtExpYearTo);
                    $('#NoOfVacancies').val(k.txtNoOfVacancies);
                    $('#RelaxationAge').val(k.txtRelaxationAge);
                    $('#CriteriaName').val(k.CriteriaName);
                    $('#CategorylistNew').P2BSelectMenuAppend("@Url.Action("GetLookupValue", "lookup")", 104, k.CategoryList);
                    $('#ExpFilterlist1').P2BSelectMenuAppend("@Url.Action("GetLookupValue", "lookup")", 1061, k.ExpFilterList);
                    $('#Genderlist').P2BSelectMenuAppend("@Url.Action("GetLookupValue", "lookup")", 129, k.GenderList);
                    $('#MaritalStatuslist').P2BSelectMenuAppend("@Url.Action("GetLookupValue", "lookup")", 128, k.MaritalStatusList);
                    $('#RangeFilterList').P2BSelectMenuAppend("@Url.Action("GetLookupValue", "lookup")", 1058, k.RangeFilterList);
                    $('#SpecialCategorylist').P2BSelectMenuAppend("@Url.Action("GetLookupValue", "lookup")", 514, k.SpecialCategoryList);
                });
                //}, 3000);
                $.each(data[1], function (i, k) {
                    ////;
                    $('#Qualificationlist').P2BLookUpEncapsulate('#Qualificationlist', 'Qualificationlist', k.Qualification_id, k.Qualification_details, 'Qualificationlist', '#Edit-Qualification-ShortlistingCriteria,#Remove-Qualification-ShortlistingCriteria', 'A');
