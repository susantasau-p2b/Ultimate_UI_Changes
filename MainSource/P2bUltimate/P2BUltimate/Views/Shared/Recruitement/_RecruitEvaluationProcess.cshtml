            //dt = $('#JqGrid').P2BGrid.P2BEdDelV('#JqGrid', 'Id');
            //var a = [], b;
            //a = $('#RecruitShortlistingParalist').P2BGetTableDataonSelectedRow('#RecruitShortlistingParalist', 0);
            //b = a[0];

            var selectedRows = [];
            selectedRows = $('#RecruitShortlistingParalist').P2BGetTableDataonSelectedRow('#RecruitShortlistingParalist', 0);
            $('<div></div>').P2BPartialDeleteModalDialog('', selectedRows, 0, 'Are you sure want to delete record???', 'Message : ', 'Request failed', '#RecruitShortlistingParalist', $('#RecruitShortlistingParalist').val(), 200, 200, '#FormRecruitEvaluationProcess', '#RecruitShortlistingParalist', '#Edit-ShortlistingCriteria-RecruitEvaluationProcess,#Remove-ShortlistingCriteria-RecruitEvaluationProcess');
            event.preventDefault();
        });

        $('#Autho').makeDisable('#Create,#Edit,#Delete');
        $('.popup_radio_button').controlgroup();
        $('#Autho').onClickGrid('#JqGrid', '@Url.Action("P2BGrid")', '@Url.Action("P2BGrid")');
        //$('.drop').selectmenu().selectmenu('menuWidget');
    });
</script>
