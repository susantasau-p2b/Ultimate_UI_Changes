                    $('#FuncStructlist').P2BLookUpEncapsulate('#FuncStructlist', 'FuncStructlist', k.FuncStruct_Id, k.FuncStruct_val, 'FuncStructlist', '#Remove-FuncStruct-ShortlistingCriteria', 'N');
                });

