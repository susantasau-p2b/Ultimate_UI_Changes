@{
    Layout = null;
}
@model Recruitment.RecruitJoiningPara
<style>
    .radio_button_div_1, .horizontal_increpoly_row1 {
        display: inline-block;
    }

    .increpoly_popup-content-label {
        display: block;
        float: left;
        width: 123px;
    }

    .increpoly_popup-content-textbox {
        width: 120px;
    }

    .increpoly_row1 {
        width: 424px;
    }

    .increpoly_icon-row {
        position: relative;
        left: 81px;
        top: -150px;
    }
</style>
<script>
	$(document).ready(function (e) {
		var dt;
		$('button').button();
		$('#Stage_Join').ForceAmount().MaxLength(8);


	    // Partial page edit
		$('.drop').selectmenu().selectmenu('menuWidget');
		$('#RecruitJoinParalist').selectmenu().selectmenu("menuWidget");
		$('#RecruitJoinParalist').P2BSelectMenuAppend("@Url.Action("GetLookupValue", "lookup")", 701, "");
		$('#Autho').makeDisable('#Create,#Edit,#Delete');
		$('.popup_radio_button').controlgroup();
		$('#Autho').onClickGrid('#JqGrid', '@Url.Action("P2BGrid")', '@Url.Action("P2BGrid")');
		//$('.drop').selectmenu().selectmenu('menuWidget');
	});
</script>

<div id="Create_Dialog" title="Create JobAgency">
    @using (Html.BeginForm(null, null, FormMethod.Post, new { Id = "frmRecruitJoiningPara", name = "frmRecruitJoiningPara" }))
    {
        <input id="Autho_Action" type="hidden" name="Autho_Action" />
        <input id="Autho_Allow" type="hidden" name="Autho_Allow" />
        //<div class="increpoly_row1 content_margine">@Html.LabelFor(m => m.Name, "Name :", new { @class = "increpoly_popup-content-label" })@Html.TextBoxFor(m => m.Name, new { @class = "increpoly_popup-content-textbox" }) </div>

        <div class="row3 content_margine">@Html.LabelFor(m => m.Stage, "Stage :", new { @class = "popup-content-label must" })@Html.TextBoxFor(m => m.Stage, new { @class = "popup-content-textbox must" ,@id = "Stage_Join"})</div>
      
        <div class="dropdown_row1 row3 content_margine">@Html.LabelFor(m => m.RecruitJoinPara, "Recruit Join Para:", new { @class = "popup-content-label must" })<select id="RecruitJoinParalist" name="RecruitJoinParalist" class="drop must" style="width:161px;"><option></option></select></div>

      

       



    }
</div>
