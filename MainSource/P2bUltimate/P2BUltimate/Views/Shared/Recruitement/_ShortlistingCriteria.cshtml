@{
    Layout = null;
}
@model Recruitment.ShortlistingCriteria
<style>
    .icon-row {
        position: relative;
        left: 76px;
        top: -150px;
    }

    #BusinessType-button {
        font-size: 9px;
        padding-top: 1px;
        font-weight: bold;
    }

    .popup-content-label {
        display: block;
        float: left;
        width: 121px;
    }

    .popup-content-textbox {
        margin-bottom: 1px;
        min-width: 150px;
    }

    .row3 {
        width: 450px;
    }

    .row4 {
        position: absolute;
        top: 294px;
        width: 400px;
    }

    .popup-content-textbox-long {
        margin-bottom: 1px;
        width: 290px;
    }


    .row2 {
        width: 402px;
    }
</style>


<div id="Create_Dialog" title="Create JobAgency">
    @using (Html.BeginForm(null, null, FormMethod.Post, new { Id = "FormShortlistingCriteria", name = "FormShortlistingCriteria" }))
    {
        <input id="Autho_Action" type="hidden" name="Autho_Action" />
        <input id="Autho_Allow" type="hidden" name="Autho_Allow" />
        //<div class="increpoly_row1 content_margine">@Html.LabelFor(m => m.Name, "Name :", new { @class = "increpoly_popup-content-label" })@Html.TextBoxFor(m => m.Name, new { @class = "increpoly_popup-content-textbox" }) </div>
         <div class="dropdown_row1 row3 content_margine">@Html.LabelFor(m => m.RangeFilter, "RangeFilter :", new { @class = "popup-content-label must" })<select id="RangeFilterList" name="RangeFilterList_DDL" class="must" style="width:161px;"><option></option></select></div>
        <div class="row3 content_margine">@Html.LabelFor(m => m.AgeFrom, "AgeFrom :", new { @class = "popup-content-label must" })@Html.TextBoxFor(m => m.AgeFrom, new { @class = "popup-content-textbox must" })</div>
        <div class="row3 content_margine">@Html.LabelFor(m => m.AgeTo, "AgeTo :", new { @class = "popup-content-label must" })@Html.TextBoxFor(m => m.AgeTo, new { @class = "popup-content-textbox must" })</div>

        <div class="dropdown_row1 row3 content_margine">
            @Html.LabelFor(m => m.Category, "Category :", new { @class = "popup-content-label must" })
            <select id="CategorylistNew" name="CategorylistNew" class="must" style="width:161px;"><option></option></select>
        </div>

        <div class="row3 content_margine">@Html.LabelFor(m => m.CriteriaName, "Criteria Name :", new { @class = "popup-content-label must" })@Html.TextBoxFor(m => m.CriteriaName, new { @class = "popup-content-textbox must" })</div>

        <div class="dropdown_row1 row3 content_margine">
            @Html.LabelFor(m => m.ExpFilter, "ExpFilter :", new { @class = "popup-content-label must" })
            <select id="ExpFilterlist1" name="ExpFilterlist1" class="must" style="width:161px;"><option></option></select>
        </div>

        <div class="row3 content_margine">@Html.LabelFor(m => m.ExpYearFrom, "Exp Year From :", new { @class = "popup-content-label must" })@Html.TextBoxFor(m => m.ExpYearFrom, new { @class = "popup-content-textbox must" })</div>

        <div class="row3 content_margine">@Html.LabelFor(m => m.ExpYearTo, "Exp Year To :", new { @class = "popup-content-label must" })@Html.TextBoxFor(m => m.ExpYearTo, new { @class = "popup-content-textbox must" })</div>

        <div class="row3 content_margine">
            <div id="FuncStruct_old"></div>@Html.LabelFor(m => m.FuncStruct, "Func Struct :", new { @class = "popup-content-label" })
            <div class="popup-content-table">
                <table class="lookuptableselected" id="FuncStructlist">
                    <tr><th>Id</th><th>LookUp Selected Description</th></tr>
                </table>
            </div>
            <div class="icon-row">
                <button data-p2btooltip="Lookup FuncStruct Details" class="popup-content-icon-lookup" id="Lookup-FuncStruct-ShortlistingCriteria"><span class="ui-icon ui-icon-folder-open popup-content-iconmark"></span></button>
                @*<button data-p2btooltip="Create FuncStruct Details" class="popup-content-icon-create" id="Create-FuncStruct-ShortlistingCriteria"><span class="ui-icon ui-icon-newwin popup-content-iconmark"></span></button>
                    <button data-p2btooltip="Edit FuncStruct Details" class="popup-content-icon-edit" id="Edit-FuncStruct-ShortlistingCriteria"><span class="ui-icon ui-icon-pencil popup-content-iconmark"></span></button>*@
                <button data-p2btooltip="Remove FuncStruct Details" class="popup-content-icon-remove" id="Remove-FuncStruct-ShortlistingCriteria"><span class="ui-icon ui-icon-trash popup-content-iconmark"></span></button>
            </div>
        </div>

        <div class="dropdown_row1 row3 content_margine">@Html.LabelFor(m => m.Gender, "Gender :", new { @class = "popup-content-label must" })<select id="Genderlist" name="Genderlist" class="must" style="width:161px;"><option></option></select></div>

        <div class="dropdown_row1 row3 content_margine">@Html.LabelFor(m => m.MaritalStatus, "Marital Status :", new { @class = "popup-content-label must" })<select id="MaritalStatuslist" name="MaritalStatuslist" class="must" style="width:161px;"><option></option></select></div>

        <div class="row3 content_margine">@Html.LabelFor(m => m.Narration, "Narration :", new { @class = "popup-content-label must" })@Html.TextBoxFor(m => m.Narration, new { @class = "popup-content-textbox must", @id = "username" })</div>

        <div class="row3 content_margine">@Html.LabelFor(m => m.NoOfVacancies, "NoOfVacancies :", new { @class = "popup-content-label must" })@Html.TextBoxFor(m => m.NoOfVacancies, new { @class = "popup-content-textbox must" })</div>

        <div class="row3 content_margine">
            <div id="Qualification_old"></div>@Html.LabelFor(m => m.Qualification, "Qualification :", new { @class = "popup-content-label" })
            <div class="popup-content-table">
                <table class="lookuptableselected" id="Qualificationlist">
                    <tr><th>Id</th><th>LookUp Selected Description</th></tr>
                </table>
            </div>
            <div class="icon-row">
                <button data-p2btooltip="Lookup Qualification Details" class="popup-content-icon-lookup" id="Lookup-Qualification-ShortlistingCriteria"><span class="ui-icon ui-icon-folder-open popup-content-iconmark"></span></button>
                <button data-p2btooltip="Create Qualification Details" class="popup-content-icon-create" id="Create-Qualification-ShortlistingCriteria" style="top:23px;"><span class="ui-icon ui-icon-newwin popup-content-iconmark"></span></button>
                <button data-p2btooltip="Edit Qualification Details" class="popup-content-icon-edit" id="Edit-Qualification-ShortlistingCriteria"><span class="ui-icon ui-icon-pencil popup-content-iconmark"></span></button>
                <button data-p2btooltip="Remove Qualification Details" class="popup-content-icon-remove" id="Remove-Qualification-ShortlistingCriteria"><span class="ui-icon ui-icon-trash popup-content-iconmark"></span></button>
            </div>
        </div>

        @*<div class="dropdown_row1 row3 content_margine">@Html.LabelFor(m => m.RangeFilter, "RangeFilter :", new { @class = "popup-content-label must" })<select id="RangeFilterList" name="RangeFilterList_DDL" class="must" style="width:161px;"><option></option></select></div>*@

        <div class="row3 content_margine">@Html.LabelFor(m => m.RelaxationAge, "Relaxation Age :", new { @class = "popup-content-label must" })@Html.TextBoxFor(m => m.RelaxationAge, new { @class = "popup-content-textbox must" })</div>

