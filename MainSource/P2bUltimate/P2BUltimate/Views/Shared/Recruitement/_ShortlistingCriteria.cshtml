            <div id="Skill_old"></div>@Html.LabelFor(m => m.Skill, "Skill :", new { @class = "popup-content-label" })
            <div class="popup-content-table">
                <table class="lookuptableselected" id="Skilllist">
                    <tr><th>Id</th><th>LookUp Selected Description</th></tr>
                </table>
            </div>
            <div class="icon-row">
                <button data-p2btooltip="Lookup Skill Details" class="popup-content-icon-lookup" id="Lookup-Skill-ShortlistingCriteria"><span class="ui-icon ui-icon-folder-open popup-content-iconmark"></span></button>
                <button data-p2btooltip="Create Skill Details" class="popup-content-icon-create" id="Create-Skill-ShortlistingCriteria" style="top:23px;"><span class="ui-icon ui-icon-newwin popup-content-iconmark"></span></button>
                <button data-p2btooltip="Edit Skill Details" class="popup-content-icon-edit" id="Edit-Skill-ShortlistingCriteria"><span class="ui-icon ui-icon-pencil popup-content-iconmark"></span></button>
                <button data-p2btooltip="Remove Skill Details" class="popup-content-icon-remove" id="Remove-Skill-ShortlistingCriteria"><span class="ui-icon ui-icon-trash popup-content-iconmark"></span></button>
            </div>
        </div>

        <div class="dropdown_row1 row3 content_margine">@Html.LabelFor(m => m.SpecialCategory, "Special Category :", new { @class = "popup-content-label must" })<select id="SpecialCategorylist" name="SpecialCategorylist" class="must" style="width:161px;"><option></option></select></div>

