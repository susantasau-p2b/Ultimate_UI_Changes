            $('.dialog').P2BLookUpModal('@Url.Action("GetLookup", "TrainingInstitute")', '', 'LookupDiv', 'ContactDetails Look Up Data', '#TrainingInstitutelist', 'TrainingInstitutelist', 5, 'TrainingInstitutelist', '#frmFacultyExternal', 'N', '#Edit-TrainingInstitute-FacultyExternal,#Remove-TrainingInstitute-FacultyExternal', 'Lookup_table', 'PageNavPosition');
            event.preventDefault();
        });*@

        $('<div></div>').P2BPartialCreateModalDialog();
        $('#Create-TrainingInstitute-FacultyExternal').button().on("click", function (event) {
            $('<div></div>').P2BPartialCreateModalDialog('@Url.Action("partial", "TrainingInstitute")', 'Create TrainingInstitute Details', 'open', '@Url.Action("Create", "TrainingInstitute")', '#frmTrainingInstitute', 'Message :', 'Request Falied :', '#JqGrid', 500, 650, '', '', '', '#frmFacultyExternal', '', function (data) {
                $('#TrainingInstitutelist').P2BLookUpEncapsulate('#TrainingInstitutelist', 'TrainingInstitutelist', data[0], data[1], 'TrainingInstitutelist', '#Edit-ContactDetails-Disease,#Remove-ContactDetails-Disease', 'A');
                $('#InstituteTypelist').P2BSelectMenuAppend("@Url.Action("GetLookupValue", "lookup")", 1053, "");
            });
            event.preventDefault();
        });


        $('#Edit-TrainingInstitute-FacultyExternal').button().on("click", function (event) {
            var a = [], b;
            a = $('#TrainingInstitutelist').P2BGetTableDataonSelectedRow('#TrainingInstitutelist', 0);
            b = a[0];
            console.log(b);
            //url, openurl, opendataforward, editurl, maindialogtitle, forwardserializedata, forwarddata, editmessage, editerrormessage, gridreloadname,                                                                                                                    height, width, classoridoftheonwhichpopupderived, nameidclassofbuttontodisable, nameclassidofinlinelookup, nameofthelist_inlinelookuptable, nameoftable_inlinelookuptable,multiple_allowed_or_not, returndatafunction) {
            $('<div></div>').P2BPatialEditModalDialog('@Url.Action("partial", "TrainingInstitute")', '@Url.Action("Edit", "TrainingInstitute")', b, '@Url.Action("EditSave", "TrainingInstitute")', 'Edit TrainingInstitute Details', '#frmTrainingInstitute', b, 'Message :', 'Request Falied :', '#JqGrid', 500, 650, '#frmFacultyExternal', '#TrainingInstitutelist', 'TrainingInstitutelist', 'TrainingInstitutelist', 'N', '#Remove-TrainingInstitute-FacultyExternal,#Edit-TrainingInstitute-FacultyExternal', function (data) {
                // setTimeout(function () {
                $.each(data[0], function (i, k) {
                    $('#frmTrainingInstitute').trigger('reset');
                    $('#Code').val(k.Code);
                    $('#Name').val(k.Name);
                    // $('#InstituteType').val(k.InstituteType);
                    $('#ContactPerson').val(k.ContactPerson);
                    $('#InstituteTypelist').P2BSelectMenuAppend("@Url.Action("GetLookupValue", "lookup")", 1053, k.InstituteType_ID);

                });

                $.each(data[1], function (i, k) {
                    $('#ContactDetailsList').P2BLookUpEncapsulate('#ContactDetailsList', 'ContactDetailsList', k.ContactDetails_Id, k.ContactDetails_FullDetails, 'ContactDetailsList', '#Edit-ContactNo-ContDetails,#Remove-ContactNo-ContDetails', 'A');
                });
                // }, 3000);
            });
            event.preventDefault();
        });


        $('#Remove-TrainingInstitute-FacultyExternal').button().on('click', function (event) {
            dt = $('#JqGrid').P2BGrid.P2BEdDelV('#JqGrid', 'Id');
            var a = [], b;
            a = $('#TrainingInstitutelist').P2BGetTableDataonSelectedRow('#TrainingInstitutelist', 0);
            b = a[0];
            console.log(b);
            $('<div></div>').P2BPartialDeleteModalDialog('', b, 0, 'Are you sure want to delete record???', 'Message : ', 'Request failed', '#TrainingInstitutelist', $('#TrainingInstitutelist').val(), 200, 200, '#frmFacultyExternal', '#TrainingInstitutelist');
            event.preventDefault();
        });

        $('#Categorylist').selectmenu().selectmenu("menuWidget");
        $('#Categorylist').P2BSelectMenuAppend('@Url.Action("GetLookupValue", "lookup")', 100, "");
        $('.popup_radio_button').controlgroup();
        $('#Autho').makeDisable('#Create,#Edit,#Delete');

      //  $('.drop').selectmenu().selectmenu("menuWidget");
        $('#Autho').onClickGrid('#JqGrid', '@Url.Action("P2BGrid")', '@Url.Action("P2BGrid")');
    });
</script>

