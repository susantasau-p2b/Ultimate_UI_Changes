          $('.popup_radio_button').controlgroup();
        $('#Lookup-FacultySpecialization-FacultyExternal').button().on('click', function (event) {
            $('.dialog').P2BLookUpModal('@Url.Action("GetLookup", "FacultySpecialization")', '', 'LookupDiv', 'FacultySpecialization Look Up Data', '#FacultySpecializationlist', 'FacultySpecializationlist', 5, 'FacultySpecializationlist', '#frmFacultyExternal', 'N', '#Edit-FacultySpecialization-FacultyExternal,#Remove-FacultySpecialization-FacultyExternal', 'Lookup_table', 'PageNavPosition');
            event.preventDefault();
        });
        $('<div></div>').P2BPartialCreateModalDialog();
        $('#Create-FacultySpecialization-FacultyExternal').button().on("click", function (event) {
            $('<div></div>').P2BPartialCreateModalDialog('@Url.Action("partial", "FacultySpecialization")', 'Create FacultySpecialization', 'open', '@Url.Action("Create","FacultySpecialization")', '#frmFacultySpecialization', 'Message :', 'Request Falied :', '#JqGrid', 400, 500, '', '', '', '#frmFacultyExternal', '#Edit-FacultySpecializationNo-ContDetails,#Remove-FacultySpecializationNo-ContDetails', function (data) {
                $('#FacultySpecializationlist').P2BLookUpEncapsulate('#FacultySpecializationlist', 'FacultySpecializationlist', data[0], data[1], 'FacultySpecializationlist', '#Edit-FacultySpecialization-FacultyExternal,#Remove-FacultySpecialization-FacultyExternal', 'N');
            });
            event.preventDefault();
        });

        $('#Edit-FacultySpecialization-FacultyExternal').button().on("click", function (event) {
            var a = [], b;
            a = $('#FacultySpecializationlist').P2BGetTableDataonSelectedRow('#FacultySpecializationlist', 0);
            b = a[0];
            $('<div></div>').P2BPatialEditModalDialog('@Url.Action("partial", "FacultySpecialization")', '@Url.Action("Edit", "FacultySpecialization")', b, '@Url.Action("EditSave", "FacultySpecialization")', 'Edit FacultySpecialization Details', '#frmFacultySpecialization', b, 'Message :', 'Request Falied :', '#JqGrid', 400, 500, '#frmFacultyExternal', '#FacultySpecializationlist', 'FacultySpecializationlist', 'FacultySpecializationlist', 'N', '#Remove-FacultySpecializationNo-ContDetails,#Edit-FacultySpecializationNo-ContDetails', function (data) {
                $.each(data[0], function (i, k) {
                    $('#frmFacultySpecialization').trigger('reset');
                    $('#SpecType').val(k.SpecType);
                    $('#SpecArea').val(k.SpecArea);
                });

            });
            event.preventDefault();
        });

        $('#Remove-FacultySpecialization-FacultyExternal').button().on('click', function (event) {
            var a = [], b;
            a = $('#FacultySpecializationlist').P2BGetTableDataonSelectedRow('#FacultySpecializationlist', 0);
            b = a[0];
            $('<div></div>').P2BPartialDeleteModalDialog('', b, 0, 'Are you sure want to delete record???', 'Message : ', 'Request failed', '#FacultySpecializationlist', $('#FacultySpecializationlist').val(), 200, 200, '#frmFacultyExternal', '#FacultySpecializationlist', '#Edit-FacultySpecialization-FacultyExternal,#Remove-FacultySpecialization-FacultyExternal');
            event.preventDefault();
        });
        $('#Lookup-TrainingInstitute-FacultyExternal').button().on('click', function (event) {
            $('.dialog').P2BLookUpModal('@Url.Action("GetLookup", "TrainingInstitute")', '', 'LookupDiv', 'ContactDetails Look Up Data', '#TrainingInstitutelist', 'TrainingInstitutelist', 5, 'TrainingInstitutelist', '#frmFacultyExternal', 'N', '#Edit-TrainingInstitute-FacultyExternal,#Remove-TrainingInstitute-FacultyExternal', 'lookupdiv_FacultyExternal', 'pageNavPosition_FacultyExternal');
